  <SimulationDescriptor Name="AffinitySeaDemons"      Type="Affinity">
    <SimulationPropertyDescriptors>
      <!-- Mark Of The Damned equipment is unlocked by the faction quest -->
      <SimulationPropertyDescriptor Name="MarkOfTheDamnedUnitProductionCostReduction"              BaseValue="0.5" MinValue="0"/>
    </SimulationPropertyDescriptors>
    <SimulationModifierDescriptors>
      <!-- Advanced transport -->
      <SimulationModifierDescriptor TargetProperty="AttributeAttackOnWater"             Operation="Force"   Value="$(AttributeAttackOnWater_AdvancedTransport)"             Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="AttributeDefenseOnWater"            Operation="Force"   Value="$(AttributeDefenseOnWater_AdvancedTransport)"            Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="AttributeInitiativeOnWater"         Operation="Force"   Value="$(AttributeInitiativeOnWater_AdvancedTransport)"         Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="AttributeDamageOnWater"             Operation="Force"   Value="$(AttributeDamageOnWater_AdvancedTransport)"             Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="MaximumHealthOnWater"               Operation="Force"   Value="$(MaximumHealthOnWater_AdvancedTransport)"               Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="BaseBattleMaximumMovementOnWater"   Operation="Force"   Value="$(BaseBattleMaximumMovementOnWater_AdvancedTransport)"   Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="VisionRangeOnWater"                 Operation="Force"   Value="$(VisionRangeOnWater_AdvancedTransport)"                 Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>
      <SimulationModifierDescriptor TargetProperty="VisionHeightOnWater"                Operation="Force"   Value="$(VisionHeightOnWater_AdvancedTransport)"                Priority="1"      Path="ClassEmpire/Garrison/TransportShipUnit"   IsBindOnSource="false"/>

      <SimulationModifierDescriptor TargetProperty="MaximumMovementOnWater"             Operation="Force"   Value="$(MaximumMovementOnWater_AdvancedTransport)"             Priority="1"      Path="ClassEmpire/Garrison/ClassUnit,!SeafaringUnit"           IsBindOnSource="false"/>
    </SimulationModifierDescriptors>
  </SimulationDescriptor>
    

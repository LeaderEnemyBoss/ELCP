  <SimulationDescriptor Name="AffinityLesser"         Type="Affinity">
    <SimulationPropertyDescriptors>
      <!-- Unit health Regen-->
      <SimulationPropertyDescriptor Name="GlobalUnitRegenImprovement"          BaseValue="0.05"/>
      <SimulationPropertyDescriptor Name="InAlliedRegionUnitRegenModifier"     BaseValue="0"/>
      <SimulationPropertyDescriptor Name="InOwnedRegionUnitRegenModifier"      BaseValue="0"/>
      <SimulationPropertyDescriptor Name="InNoneAlliedRegionUnitRegenModifier" BaseValue="0"/>
      <SimulationPropertyDescriptor Name="UnassignedHeroRegenModifier"         BaseValue="0"/>

      <SimulationPropertyDescriptor Name="MinorEmpireDifficultyFactor"         BaseValue="1"/>
    </SimulationPropertyDescriptors>

    <SimulationModifierDescriptors>
      <SimulationModifierDescriptor TargetProperty="InAlliedRegionUnitRegenModifier"     Operation="Addition" Value="$(GlobalUnitRegenImprovement)"/>
      <SimulationModifierDescriptor TargetProperty="InOwnedRegionUnitRegenModifier"      Operation="Addition" Value="$(GlobalUnitRegenImprovement)"/>
      <SimulationModifierDescriptor TargetProperty="InNoneAlliedRegionUnitRegenModifier" Operation="Addition" Value="$(GlobalUnitRegenImprovement)"/>
      <SimulationModifierDescriptor TargetProperty="UnassignedHeroRegenModifier"         Operation="Addition" Value="$(GlobalUnitRegenImprovement)"/>

      <!-- For MinorSlayer -->
      <SimulationModifierDescriptor TargetProperty="ControlledByMinorAI"                 Operation="Addition" Value="1" Path="ClassEmpire/Garrison/ClassUnit"/>
    </SimulationModifierDescriptors>
  </SimulationDescriptor>


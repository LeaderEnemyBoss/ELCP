    <AIParameter Name="WinterPercentIndustryPenalty"                Value="0.25" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyIndustry2">
    <AIParameter Name="WinterPercentIndustryPenalty"                Value="0.25" />
  </AIParameterDatatableElement>

  <!-- ################################################################################ -->
  <!-- ##################################### DUST ##################################### -->
  <!-- ################################################################################ -->


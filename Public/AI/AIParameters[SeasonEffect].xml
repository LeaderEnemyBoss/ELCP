<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- ################################################################################ -->
  <!-- ##################################### FOOD ##################################### -->
  <!-- ################################################################################ -->

  <!-- -25% food on cities -->
  <AIParameterDatatableElement Name="WinterEffectEconomyFood1">
    <AIParameter Name="WinterPercentFoodPenalty"                    Value="0.25"/>
  </AIParameterDatatableElement>

  <!-- -1 food on districts -->
  <AIParameterDatatableElement Name="WinterEffectEconomyFood2">
    <AIParameter Name="WinterTerrainFoodPenalty"                    Value="1"/>
  </AIParameterDatatableElement>

  <!-- -50% food on cities -->
  <AIParameterDatatableElement Name="WinterEffectEconomyFood3">
    <AIParameter Name="WinterPercentFoodPenalty"                    Value="0.5"/>
  </AIParameterDatatableElement>

  <!-- -50% food on cities -->
  <AIParameterDatatableElement Name="WinterEffectEconomyFood4">
    <AIParameter Name="WinterPercentFoodPenalty"                    Value="0.5"/>

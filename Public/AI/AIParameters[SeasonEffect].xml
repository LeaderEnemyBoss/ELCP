  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyDust3">
    <AIParameter Name="WinterPercentUpkeepPenalty"                  Value="1" />
  </AIParameterDatatableElement>

  <!-- ################################################################################ -->
  <!-- #################################### FIDSI ##################################### -->
  <!-- ################################################################################ -->

  <AIParameterDatatableElement Name="WinterEffectEconomyFIDS1">
    <AIParameter Name="WinterRiverFoodPenalty"                      Value="1" />
    <AIParameter Name="WinterRiverDustPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyFIDS2">
    <AIParameter Name="WinterRiverFoodPenalty"                      Value="1" />
    <AIParameter Name="WinterRiverDustPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyFIDS3">
    <AIParameter Name="WinterExploitationSciencePenalty"            Value="2" />
    <AIParameter Name="WinterExploitationDustPenalty"               Value="2" />
    <AIParameter Name="WinterExploitationIndustryPenalty"           Value="2" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyFIDS4">
    <AIParameter Name="WinterExploitationSciencePenalty"            Value="2" />
    <AIParameter Name="WinterExploitationDustPenalty"               Value="2" />
    <AIParameter Name="WinterExploitationIndustryPenalty"           Value="2" />
  </AIParameterDatatableElement>

  <!-- ################################################################################ -->
  <!-- ################################## APPROVAL #################################### -->
  <!-- ################################################################################ -->

  <AIParameterDatatableElement Name="WinterEffectEconomyApproval1">
    <AIParameter Name="WinterApprovalPenalty"                       Value="15" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectEconomyApproval2">
    <AIParameter Name="WinterApprovalPenalty"                       Value="25" />
  </AIParameterDatatableElement>

  <!-- ################################################################################ -->
  <!-- ################################# RESOURCES #################################### -->
  <!-- ################################################################################ -->

  <AIParameterDatatableElement Name="WinterEffectEconomyResource1">
    <AIParameter Name="WinterNetStrategicPenalty"                   Value="1" />
    <AIParameter Name="WinterNetLuxuryPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <!-- ################################################################################ -->
  <!-- ############################# UNIT ATTRIBUTES ################################## -->
  <!-- ################################################################################ -->

  <AIParameterDatatableElement Name="WinterEffectMilitaryAttribute1">
    <AIParameter Name="WinterPercentUnitRegenPenalty"               Value="0.1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectMilitaryAttribute2">
    <AIParameter Name="WinterPercentUnitAttackPenalty"              Value="0.3" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectMilitaryAttribute3">
    <AIParameter Name="WinterPercentUnitDefensePenalty"             Value="0.3" />  
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectMilitaryAttribute4">
    <AIParameter Name="WinterPercentUnitLifePenalty"                Value="0.2" /> 
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="WinterEffectMilitaryMovement1">
    <AIParameter Name="WinterPercentUnitMovementPenalty"            Value="0.25" />
    <AIParameter Name="WinterPercentUnitBattleMovementPenalty"      Value="0.5" />
  </AIParameterDatatableElement>

  <!-- ############################################################################### New Winter Effects ############################################################################### -->

  <!-- ##################################### -->
  <!-- ############# Movement ############## -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectMovement2">
    <AIParameter Name="WinterPercentUnitMovementPenalty"            Value="0.25" />
    <AIParameter Name="WinterPercentUnitBattleMovementPenalty"      Value="0.25" />   
  </AIParameterDatatableElement>

  <!-- +25% Movement Cost out of Forests, on land tiles / -50% Movement Cost on water tiles -->
  <AIParameterDatatableElement Name="SeasonEffectMovement3">
    <AIParameter Name="WinterPercentUnitMovementPenalty"            Value="0.25" />
    <AIParameter Name="WinterPercentShipMovementBonus"              Value="0.5" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############# FIDS Tile ############# -->
  <!-- ##################################### -->

  <!-- -1 Dust on tiles -->
  <AIParameterDatatableElement Name="SeasonEffectFIDSTile1">
    <AIParameter Name="WinterTerrainFoodPenalty"                    Value="1" />
  </AIParameterDatatableElement>

  <!-- -1 Industry on exploitation -->
  <AIParameterDatatableElement Name="SeasonEffectFIDSTile3">
    <AIParameter Name="WinterExploitationIndustryPenalty"           Value="1" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ FIDS per pop ########### -->
  <!-- ##################################### -->

  <!--Only for Harsh & Anarchic Winters-->
  <AIParameterDatatableElement Name="SeasonEffectFIDSPerPop1">
    <AIParameter Name="WinterPerPopulationFoodPenalty"              Value="1" />
  </AIParameterDatatableElement>
  
  <!--Only for Harsh & Anarchic Winters-->
  <AIParameterDatatableElement Name="SeasonEffectFIDSPerPop2">
    <AIParameter Name="WinterPerPopulationIndustryPenalty"          Value="1" />
  </AIParameterDatatableElement>

  <!--Only for Harsh & Anarchic Winters-->
  <AIParameterDatatableElement Name="SeasonEffectFIDSPerPop3">
    <AIParameter Name="WinterPerPopulationDustPenalty"              Value="1" />
  </AIParameterDatatableElement>

  <!--Only for Harsh & Anarchic Winters-->
  <AIParameterDatatableElement Name="SeasonEffectFIDSPerPop4">
    <AIParameter Name="WinterPerPopulationSciencePenalty"           Value="1" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ########## Anomaly Terrain ########## -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectAnomalyTerrain1">
    <AIParameter Name="WinterAnomalyFoodPenalty"                    Value="3" />
    <AIParameter Name="WinterAnomalyIndustryBonus"                  Value="3" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectAnomalyTerrain2">
    <AIParameter Name="WinterAnomalyApprovalPenalty"                Value="10" />
    <AIParameter Name="WinterAnomalyDustBonus"                      Value="3" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ City Defense ########### -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectCityDefense1">
    <AIParameter Name="WinterCityUnitMoralePenalty"                 Value="3" />
  </AIParameterDatatableElement>

  <!-- -25% City Fortification / +50% Fortification Damage -->
  <AIParameterDatatableElement Name="SeasonEffectCityDefense2ReplicantsPack">
    <AIParameter Name="WinterPercentCityDefensePenalty"             Value="0.6" />
    <AIParameter Name="WinterPercentPillageDefensePenalty"          Value="0.6" />
  </AIParameterDatatableElement>

  <!-- -25% City Fortification / +50% Siege power -->
  <AIParameterDatatableElement Name="SeasonEffectCityDefense2">
    <AIParameter Name="WinterPercentCityDefensePenalty"             Value="0.6" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ Frozen Water ########### -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectFrozenWater1">
    <AIParameter Name="WinterIceShelf"                              Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectFrozenWater2">
    <AIParameter Name="WinterIceShelf"                              Value="1" />
    <AIParameter Name="WinterWaterFoodPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectFrozenWater3">
    <AIParameter Name="WinterIceShelf"                              Value="1" />
    <AIParameter Name="WinterWaterDustPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectFrozenWater4">
    <AIParameter Name="WinterIceShelf"                              Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectFrozenWater5">
    <AIParameter Name="WinterIceShelf"                              Value="1" />
    <AIParameter Name="WinterWaterFoodPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ######## Resource Extraction ######## -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectResourceExtraction1">
    <AIParameter Name="WinterNetStrategicPenalty"                   Value="1" />
    <AIParameter Name="WinterNetLuxuryBonus"                        Value="1" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectResourceExtraction2">
    <AIParameter Name="WinterNetStrategicBonus"                     Value="1" />
    <AIParameter Name="WinterNetLuxuryPenalty"                      Value="1" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ########## Strategic Costs ########## -->
  <!-- ##################################### -->

  <!-- +20% on Titanium and Glassteel item costs / -20% on Palladian and Adamantian item costs -->
  <AIParameterDatatableElement Name="SeasonEffectStrategicCost1">
    <AIParameter Name="WinterPercentCommonItemCostPenalty"          Value="0.1" />
    <AIParameter Name="WinterPercentUncommonItemCostBonus"          Value="0.1" />
  </AIParameterDatatableElement>

  <!-- +20% on Palladian and Adamantian item costs / -20% on Mythrite and Hyperium item costs -->
  <AIParameterDatatableElement Name="SeasonEffectStrategicCost2">
    <AIParameter Name="WinterPercentUncommonItemCostPenalty"        Value="0.1" />
    <AIParameter Name="WinterPercentRareItemCostBonus"              Value="0.1" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ####### Battle Unit Attributes ###### -->
  <!-- ##################################### -->

  <!-- -1 Battle Movement on Flying Units / +1 Battle Movement on Cavalry units -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes1">
    <AIParameter Name="WinterFlyingUnitPenalty"                     Value="0.25" />
    <AIParameter Name="WinterCavalryUnitBonus"                      Value="0.25" />
  </AIParameterDatatableElement>

  <!-- -1 Range on Ranged Units / +1 Range on Support Units -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes2">
    <AIParameter Name="WinterRangedUnitPenalty"                     Value="0.3" />
    <AIParameter Name="WinterSupportUnitBonus"                      Value="0.3" />
  </AIParameterDatatableElement>

  <!-- -25% Defense on Cavalry Units / +25% Defense on Infantry Units -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes3">
    <AIParameter Name="WinterInfantryUnitBonus"                     Value="0.25" />
    <AIParameter Name="WinterCavalryUnitPenalty"                    Value="0.25" />
  </AIParameterDatatableElement>

  <!-- -30% Attack on Infantry units / +30% Attack on Ranged units -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes4">
    <AIParameter Name="WinterInfantryUnitPenalty"                   Value="0.3" />
    <AIParameter Name="WinterRangedUnitBonus"                       Value="0.3" />
  </AIParameterDatatableElement>

  <!-- -25% Damage on Support Units / +25% Damage on Flying Units -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes5">
    <AIParameter Name="WinterFlyingUnitBonus"                       Value="0.25" />
    <AIParameter Name="WinterCavalryUnitPenalty"                    Value="0.25" />
  </AIParameterDatatableElement>

  <!-- No reinforcements, -50% military upkeep -->
  <AIParameterDatatableElement Name="SeasonEffectBattleUnitAttributes6">
    <!-- This large armies rather than small ones -->
    <AIParameter Name="AIEmpireMilitaryPower"                           Value="0.001" />
  </AIParameterDatatableElement>



  <!-- ##################################### -->
  <!-- ############# Diplomacy ############# -->
  <!-- ##################################### -->

  <AIParameterDatatableElement Name="SeasonEffectDiplomacy1">
    <AIParameter Name="WinterOpenBorders"                                         Value="1" />
  </AIParameterDatatableElement>

  <!-- -50% Diplomatic Points earned / -50% Influence costs on Declarations -->
  <AIParameterDatatableElement Name="SeasonEffectDiplomacy2">
    <AIParameter Name="WinterPercentPeacePointsPenalty"                           Value="0.5" />
    <AIParameter Name="WinterPercentDeclarationCostBonus"                         Value="0.5" />
  </AIParameterDatatableElement>

  <!-- -10% Regeneration in non allied region / +10% regeneration in owned and allied regions -->
  <AIParameterDatatableElement Name="SeasonEffectDiplomacy3">
    <AIParameter Name="WinterPercentUnitOnEnemyTerritoryRegenPenalty"             Value="0.1" />
    <AIParameter Name="WinterPercentUnitOnFriendlyTerritoryRegenBonus"            Value="0.1" />
  </AIParameterDatatableElement>

  <!-- -5% Empire Dust per war declared / +50% on Diplomatic Points earned -->
  <AIParameterDatatableElement Name="SeasonEffectDiplomacy4">
    <AIParameter Name="AIWorldWar"                                                Value="0 - 0.5" />
    <AIParameter Name="WinterPercentDustPenalty"                                  Value="0.05 * $Property(ClassEmpire:WarCount)" />
    <AIParameter Name="WinterPercentPeacePointsBonus"                             Value="0.5" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############# Espionage ############# -->
  <!-- ##################################### -->

  <!-- +25% on General and Governor Hero Upkeep / -100% on Infiltrated Hero Upkeep -->
  <AIParameterDatatableElement Name="SeasonEffectEspionage1">
    <AIParameter Name="WinterPercentGeneralUpkeepPenalty"                         Value="0.25" />
    <AIParameter Name="WinterPercentSpyUpkeepBonus"                               Value="1.0" />
  </AIParameterDatatableElement>

  <!-- -15 City Security -->
  <AIParameterDatatableElement Name="SeasonEffectEspionage2">
    <AIParameter Name="WinterSecurityPenalty"                                     Value="15" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############# Expansion ############# -->
  <!-- ##################################### -->

  <!-- +25% Expansion Disapproval / +25% FIDSI per pop -->
  <AIParameterDatatableElement Name="SeasonEffectExpansion1">
    <AIParameter Name="WinterPercentPerPopulationFoodBonus"                       Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationIndustryBonus"                   Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationDustBonus"                       Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationScienceBonus"                    Value="0.25" />
    <AIParameter Name="WinterPercentExpansionDisapprovalPenalty"                  Value="0.25" />
  </AIParameterDatatableElement>

  <!-- -25% FIDSI per pop / -25% Expansion Disapproval -->
  <AIParameterDatatableElement Name="SeasonEffectExpansion2">
    <AIParameter Name="WinterPercentPerPopulationFoodPenalty"                     Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationIndustryPenalty"                 Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationDustPenalty"                     Value="0.25" />
    <AIParameter Name="WinterPercentPerPopulationSciencePenalty"                  Value="0.25" />
    <AIParameter Name="WinterPercentExpansionDisapprovalBonus"                    Value="0.25" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ Experience ############# -->
  <!-- ##################################### -->

  <!-- +50% Hero XP as General / -50% Hero XP as Governor -->
  <AIParameterDatatableElement Name="SeasonEffectExperience1">
    <AIParameter Name="WinterPercentGeneralXPBonus"                               Value="0.5" />
    <AIParameter Name="WinterPercentGovernorXPPenalty"                            Value="0.5" />
  </AIParameterDatatableElement>

  <!-- -50% XP earned at unit creation / +25% XP earned on units -->
  <AIParameterDatatableElement Name="SeasonEffectExperience2">
    <AIParameter Name="WinterPercentGeneralXPPenalty"                             Value="0.5" />
    <AIParameter Name="WinterPercentGovernorXPBonus"                              Value="0.5" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ Marketplace ############ -->
  <!-- ##################################### -->

  <!-- x0 Hero Recovery Speed / -25% on Hero & Mercenary market prices -->
  <AIParameterDatatableElement Name="SeasonEffectMarketplace1">
    <AIParameter Name="WinterPercentHeroRecoveryPenalty"                          Value="1" />
    <AIParameter Name="WinterPercentMercenaryMarketPrice"                         Value="(0 - 0.25)" />
  </AIParameterDatatableElement>

  <!-- +25% Luxury Prices on Market / -25% Strategic prices on Market -->
  <AIParameterDatatableElement Name="SeasonEffectMarketplace2">
    <AIParameter Name="WinterPercentLuxuryMarketPrice"                            Value="0.25" />
    <AIParameter Name="WinterPercentStrategicMarketPrice"                         Value="(0 - 0.25)" />
  </AIParameterDatatableElement>

  <!-- +25% Strategic Resource prices on Market / -25% Luxury Resource prices on Market -->
  <AIParameterDatatableElement Name="SeasonEffectMarketplace3">
    <AIParameter Name="WinterPercentLuxuryMarketPrice"                            Value="(0 - 0.25)" />
    <AIParameter Name="WinterPercentStrategicMarketPrice"                         Value="0.25" />
  </AIParameterDatatableElement>

  <!-- +25% Food Stockpile prices on Market / -50% Industry Stockpile prices on Market (AI doesn't currently buy or sell stockpiles) -->
  <AIParameterDatatableElement Name="SeasonEffectMarketplace4">
    <AIParameter Name="AIEmpireMoney"                                             Value="0.001" />
  </AIParameterDatatableElement>

  <!-- +25% Food Stockpile prices on Market / -50% Industry Stockpile prices on Market (AI doesn't currently buy or sell stockpiles) -->
  <AIParameterDatatableElement Name="SeasonEffectMarketplace5">
    <AIParameter Name="AIEmpireMoney"                                             Value="0.001" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ####### Unit Production Cost ######## -->
  <!-- ##################################### -->

  <!-- +20% Major Faction unit production cost / -20% Minor Faction production costs -->
  <AIParameterDatatableElement Name="SeasonEffectUnitProductionCost1">
    <AIParameter Name="WinterPercentMajorFactionUnitProductionCostPenalty"            Value="0.2" />
    <AIParameter Name="WinterPercentMinorFactionUnitProductionCostBonus"              Value="0.2" />
  </AIParameterDatatableElement>

  <!-- +20% Minor Faction unit production cost / -20% Major Faction production costs -->
  <AIParameterDatatableElement Name="SeasonEffectUnitProductionCost2">
    <AIParameter Name="WinterPercentMajorFactionUnitProductionCostBonus"              Value="0.2" />
    <AIParameter Name="WinterPercentMinorFactionUnitProductionCostPenalty"            Value="0.2" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ City Upkeep ############ -->
  <!-- ##################################### -->

  <!-- -5 Dust per regional building built in cities -->
  <AIParameterDatatableElement Name="SeasonEffectCityUpkeep1">
  <AIParameter Name="WinterRegionalBuildingDustPenalty"                 Value="5" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############ Trade Routes ########### -->
  <!-- ##################################### -->

  <!-- -50% on Dust Trade Route gain / +25% on Science Trade Route gain -->
  <AIParameterDatatableElement Name="SeasonEffectTradeRoutes1">
    <AIParameter Name="WinterPercentDustTradePenalty"                   Value="0.5" />
    <AIParameter Name="WinterPercentScienceTradeBonus"                  Value="0.25" />
  </AIParameterDatatableElement>

  <!-- -50% on Science Trade Route gain / +25% on Dust Trade Route gain -->
  <AIParameterDatatableElement Name="SeasonEffectTradeRoutes2">
    <AIParameter Name="WinterPercentScienceTradePenalty"                Value="0.5" />
    <AIParameter Name="WinterPercentDustTradeBonus"                     Value="0.25" />
  </AIParameterDatatableElement>

  <!-- ##################################### -->
  <!-- ############## Special ############## -->
  <!-- ##################################### -->

  <!-- +15% on Hostile Minor Faction unit attributes / -50% Assimilation cost -->
  <AIParameterDatatableElement Name="SeasonEffectSpecial1">
    <AIParameter Name="WinterPercentHostileMinorAttributes"             Value="0.15" />
    <AIParameter Name="WinterPercentAssimilationCostBonus"              Value="0.5" />
  </AIParameterDatatableElement>

  <AIParameterDatatableElement Name="SeasonEffectSpecial2">
    <AIParameter Name="AIEmpireWorldWar"                                Value="0.2 * (1 - 2*(1 min $Count(ClassEmpire,AffinityReplicants)))" />
    <AIParameter Name="AIEmpireWorldPeace"                              Value="(0 - 0.5)" />
    <AIParameter Name="AIEmpireSpy"                                     Value="0.5 * (1 - (1 min $Count(ClassEmpire,AffinityReplicants)))" />
  </AIParameterDatatableElement>

  <!-- +50% Influence costs on Empire Plan -->
  <AIParameterDatatableElement Name="SeasonEffectSpecial3">
    <AIParameter Name="WinterPercentEmpirePlanCostPenalty"              Value="0.5" />
  </AIParameterDatatableElement>

  <!-- +25% Current Era Research Costs / -25% Previous Eras Research Costs -->
  <AIParameterDatatableElement Name="SeasonEffectSpecial4">
    <AIParameter Name="WinterPercentCurrentEraResearchCostPenalty"      Value="0.25" />
    <AIParameter Name="WinterPercentPreviousEraResearchCostBonus"        Value="0.25" />
  </AIParameterDatatableElement>
  
</Datatable>
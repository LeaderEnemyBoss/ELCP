<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <AIPersonalityDefinition Name="PersonalityAffinityMezari">
    <Multipliers>
      <!-- AI Registry -->
      <AI>

        <AgentDefinition>

          <!-- 
          Architects
          -->
          <IndustryReferenceTurnCountAgent>
            <MaximumTurnCount>0.75</MaximumTurnCount>
            <ProductionStressMinimumValue>1</ProductionStressMinimumValue>
          </IndustryReferenceTurnCountAgent>

          <!--
          Isolationists
          -->
          <PopulationReferenceTurnCountAgent>
            <!-- MaximumNumberOfTurnToReachNextPopulation = RangeRatio *  IdealNumberOfTurnToReachNextPopulation -->
            <RangeRatio>1.1</RangeRatio>
          </PopulationReferenceTurnCountAgent>

          <!--
          Science-focused
          -->
          <TechnologyReferenceTurnCountAgent>
            <IdealTechnologyUnlockPeriod>0.75</IdealTechnologyUnlockPeriod>
            <MaximumPeriodGap>0.75</MaximumPeriodGap>
          </TechnologyReferenceTurnCountAgent>
		  
		  <MoneyPrintTermAgent>
            <Multiplier>0</Multiplier>
		  </MoneyPrintTermAgent>

        </AgentDefinition>

        <MajorEmpire>
          <AIEntity_Empire>
			<AILayer_Victory>
				<VictoryFocusMilitary>1.8</VictoryFocusMilitary>
				<VictoryFocusDiplomacy>1</VictoryFocusDiplomacy>
				<VictoryFocusEconomy>1.1</VictoryFocusEconomy>
				<VictoryFocusTechnology>1.7</VictoryFocusTechnology>
			</AILayer_Victory>
			
            <!-- The degree to which various attitude modifiers are taken into account -->
            <AILayer_Attitude>
              <!-- ##### -->
              <!-- TRUST -->
              <!-- ##### -->
              <!-- Positive -->
              <AttitudeScorePeaceful>1.1</AttitudeScorePeaceful>
              <AttitudeScoreCommonEnemy>1.3</AttitudeScoreCommonEnemy>
              <AttitudeScoreCommonFriend>1.2</AttitudeScoreCommonFriend>
              <AttitudeScoreCommonAlly>1.2</AttitudeScoreCommonAlly>
              <AttitudeScorePositiveContract>0.9</AttitudeScorePositiveContract>
              <AttitudeScoreGift>1.1</AttitudeScoreGift>
              <AttitudeScoreMantaAspiratingInMyTerritory>1.1</AttitudeScoreMantaAspiratingInMyTerritory>
              <AttitudeScoreLongTermAlliance>0.9</AttitudeScoreLongTermAlliance>
              <AttitudeScoreLongTermPeace>0.9</AttitudeScoreLongTermPeace>
              <AttitudeScoreTerraformedInMyTerritoryPositive>0.25</AttitudeScoreTerraformedInMyTerritoryPositive>
              <AttitudeScoreTerraformedNearMyArmiesPositive>0.25</AttitudeScoreTerraformedNearMyArmiesPositive>
              <AttitudeScoreKaijuPlacedNearMyCityPositive>0.6</AttitudeScoreKaijuPlacedNearMyCityPositive>
              <!-- Negative -->
              <AttitudeScoreLastWar>1.8</AttitudeScoreLastWar>
              <AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>1.7</AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>
              <AttitudeScoreAggressiveColonization>1.4</AttitudeScoreAggressiveColonization>
              <AttitudeScoreNegativeContract>1.1</AttitudeScoreNegativeContract>
              <AttitudeScoreVillageConverted>1.4</AttitudeScoreVillageConverted>
              <AttitudeScoreRegionalBuildingDestroyed>1.4</AttitudeScoreRegionalBuildingDestroyed>
              <AttitudeScoreSpy>1.6</AttitudeScoreSpy>
              <AttitudeScorePillaging>1.6</AttitudeScorePillaging>
              <AttitudeScorePillageSucceed>1.6</AttitudeScorePillageSucceed>
              <AttitudeScoreDismantleDeviceSuffered>1.6</AttitudeScoreDismantleDeviceSuffered>
              <AttitudeScoreOrbsStolen>1.1</AttitudeScoreOrbsStolen>
              <AttitudeScoreEliminatedAcquaintance>1.2</AttitudeScoreEliminatedAcquaintance>
              <AttitudeScoreEliminatedAlly>1.3</AttitudeScoreEliminatedAlly>
              <AttitudeScoreEliminatedEnemy>0.8</AttitudeScoreEliminatedEnemy>
              <AttitudeScoreEliminatedFriend>0.9</AttitudeScoreEliminatedFriend>
              <AttitudeScoreCityTakenFromAlly>1.3</AttitudeScoreCityTakenFromAlly>
              <AttitudeScoreCityTakenFromEnemy>0.8</AttitudeScoreCityTakenFromEnemy>
              <AttitudeScoreCityTakenFromFriend>0.9</AttitudeScoreCityTakenFromFriend>
              <AttitudeScoreAtWarWithMyAlly>1.2</AttitudeScoreAtWarWithMyAlly>
              <AttitudeScoreAlliedWithMyEnemy>1.3</AttitudeScoreAlliedWithMyEnemy>
              <AttitudeScoreBetrayedYourAlly>0.8</AttitudeScoreBetrayedYourAlly>
              <AttitudeScoreBetrayedYourFriend>0.8</AttitudeScoreBetrayedYourFriend>
              <AttitudeScoreTerraformedInMyTerritoryNegative>0</AttitudeScoreTerraformedInMyTerritoryNegative>
              <AttitudeScoreTerraformedNearMyArmiesNegative>0</AttitudeScoreTerraformedNearMyArmiesNegative>
              <AttitudeScoreCreepingNodeUpgradeComplete>0.8</AttitudeScoreCreepingNodeUpgradeComplete>
              <AttitudeScoreKaijuAttacked>1.5</AttitudeScoreKaijuAttacked>
              <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>

              <!-- #### -->
              <!-- WAR -->
              <!-- #### -->
              <!-- Positive (towards asking for peace) -->
              <AttitudeScoreWarFatigue>0.6</AttitudeScoreWarFatigue>
              <AttitudeScoreMyEmpireUnitsKilledDuringWar>0.8</AttitudeScoreMyEmpireUnitsKilledDuringWar>
              <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>0.8</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreMyEmpireCitiesTakenDuringWar>0.8</AttitudeScoreMyEmpireCitiesTakenDuringWar>
              <AttitudeScoreMyEmpireFortressesTakenDuringWar>0.8</AttitudeScoreMyEmpireFortressesTakenDuringWar>
              <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>0.8</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
              <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>0.8</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
              <!-- Negative (towards remaining at war) -->
              <AttitudeScoreOtherEmpireUnitsKilledDuringWar>1.1</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
              <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>1.1</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreOtherEmpireCitiesTakenDuringWar>1.1</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
              <AttitudeScoreOtherEmpireFortressesTakenDuringWar>1.1</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
              
              <!-- #### -->
              <!-- FEAR -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadMilitaryPower>1.4</AttitudeScoreMyEmpireLeadMilitaryPower>
              <AttitudeScoreMyEmpireLeadNavalMilitaryPower>1.2</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreOtherEmpireLeadMilitaryPower>1.1</AttitudeScoreOtherEmpireLeadMilitaryPower>
              <AttitudeScoreUnitsInMyTerritory>0.8</AttitudeScoreUnitsInMyTerritory>
              <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>1.0</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
              <!-- #### -->
              <!-- ENVY -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadScore>0.9</AttitudeScoreMyEmpireLeadScore>
              <AttitudeScoreMyEmpireLeadRegionCount>0.6</AttitudeScoreMyEmpireLeadRegionCount>
              <AttitudeScoreMyEmpireLeadNavalRegionCount>0.9</AttitudeScoreMyEmpireLeadNavalRegionCount>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreCommonFrontier>1.2</AttitudeScoreCommonFrontier>
              <AttitudeScoreOtherEmpireLeadScore>1.3</AttitudeScoreOtherEmpireLeadScore>
              <AttitudeScoreLivingSpaceTense>1.3</AttitudeScoreLivingSpaceTense>
              <AttitudeScoreOtherEmpireLeadRegionCount>0.9</AttitudeScoreOtherEmpireLeadRegionCount>
              <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
              <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
              <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
              <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
              <AttitudeScoreVictoryAlertDiplomacy>1</AttitudeScoreVictoryAlertDiplomacy>
              <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
              <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
              <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>
              <AttitudeScoreVictoryAlertQuest>1</AttitudeScoreVictoryAlertQuest>
              <AttitudeScoreFortressesOwnedInTheSameRegion>1.4</AttitudeScoreFortressesOwnedInTheSameRegion>
              <AttitudeScoreOtherEmpireHasUniqueFacility>1.3</AttitudeScoreOtherEmpireHasUniqueFacility>
              <AttitudeScoreOtherEmpireLeadNavalRegionCount>0.9</AttitudeScoreOtherEmpireLeadNavalRegionCount>
            </AILayer_Attitude>

            <AILayer_Strategy>

              <ResearchScoreFactor>1.4</ResearchScoreFactor>
              <EconomicScoreFactor>1.2</EconomicScoreFactor>
              <DiplomaticScoreFactor>0.9</DiplomaticScoreFactor>
              <PrepareWarScoreFactor>1.1</PrepareWarScoreFactor>

              <!--
              Defensive
              -->
              <Network>
                <MilitaryEmpireNeedAgent>
                  <GlobalUnitCountMultiplier>1.5</GlobalUnitCountMultiplier>

                  <!-- City unit need: NumberOfUnitPerCity = CLAMP((turn - CityUnitNeedStartTurn) / CityUnitNeedTurnRatio, MinimumNumberOfUnitPerCity, MaximumNumberOfUnitPerCity) -->
                  <CityUnitNeedStartTurn>0.8</CityUnitNeedStartTurn>
                  <CityUnitNeedTurnRatio>0.9</CityUnitNeedTurnRatio>
                  <MinimumNumberOfUnitPerCity>0</MinimumNumberOfUnitPerCity>
                  <MaximumNumberOfUnitPerCity>1.3</MaximumNumberOfUnitPerCity>

                  <!-- Roaming unit need: NumberOfRoamingUnit = CLAMP((turn - RoamingUnitNeedStartTurn) / RoamingUnitNeedTurnRatio, MinimumNumberOfRoamingUnit, MaximumNumberOfRoamingUnit) -->
                  <RoamingUnitNeedStartTurn>0</RoamingUnitNeedStartTurn>
                  <RoamingUnitNeedTurnRatio>1.2</RoamingUnitNeedTurnRatio>
                  <MinimumNumberOfRoamingUnit>1.2</MinimumNumberOfRoamingUnit>
                  <MaximumNumberOfRoamingUnit>1.2</MaximumNumberOfRoamingUnit>

                  <!-- War unit need: NumberOfWarUnit = CLAMP(WarUnitNeedOffset + (turn - WarUnitNeedStartTurn) / WarUnitNeedTurnRatio, MinimumNumberOfWarUnit, MaximumNumberOfWarUnit) -->
                  <WarUnitNeedOffset>1.1</WarUnitNeedOffset>
                  <WarUnitNeedStartTurn>0.9</WarUnitNeedStartTurn>
                  <WarUnitNeedTurnRatio>0.9</WarUnitNeedTurnRatio>
                  <MinimumNumberOfWarUnit>1.2</MinimumNumberOfWarUnit>
                  <MaximumNumberOfWarUnit>1.2</MaximumNumberOfWarUnit>

                  <!-- NumbeOfUnitWantedInTheEmpire = (NumberOfUnitPerCity + NumberOfRoamingUnit + NumberOfWarUnit) * GlobalUnitCountMultiplier -->
                </MilitaryEmpireNeedAgent>
              </Network>
            </AILayer_Strategy>

            <!--
            Mind of metal and wheels
            -->
            <AILayer_ResourceManager>
              <MoneyFactor>0.8</MoneyFactor>
              <ProductionFactor>1.2</ProductionFactor>
              <StrategicResourceMinimumStockPerCity>1.5</StrategicResourceMinimumStockPerCity>
              <MaxDeviationFromResourceMinimumStock>0.5</MaxDeviationFromResourceMinimumStock>
            </AILayer_ResourceManager>

            <AILayer_Altar>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireGrowth>1.4</AIEmpireGrowth>
                <AIEmpireProduction>1.1</AIEmpireProduction>
                <AIEmpireMoney>1.3</AIEmpireMoney>
                <AIEmpireResearch>1.8</AIEmpireResearch>
                <AIEmpireEmpirePoint>1.1</AIEmpireEmpirePoint>
                <AIEmpireApproval>1.2</AIEmpireApproval>

                <AIEmpireMilitaryPower>1.1</AIEmpireMilitaryPower>
                <AIEmpireAntiSpy>1.0</AIEmpireAntiSpy>
                <AIEmpirePillageDefense>1.1</AIEmpirePillageDefense>
                <AIEmpireCityDefense>1.5</AIEmpireCityDefense>
                <AIEmpireStrategicResource>1.4</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.6</AIEmpireLuxuryResource>
                
                <AIWorldNavy>1.0</AIWorldNavy>
                <AIWorldSpy>0.1</AIWorldSpy>
                <AIWorldWar>1.1</AIWorldWar>
                <AIWorldPeace>0.7</AIWorldPeace>
                <AIWorldStrategicMarketPrice>-0.1</AIWorldStrategicMarketPrice>
                <AIWorldLuxuryMarketPrice>-0.1</AIWorldLuxuryMarketPrice>
                <AIWorldMercenaryMarketPrice>-0.3</AIWorldMercenaryMarketPrice>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Altar>
            
            <AILayer_Research>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireApproval>1.1</AIEmpireApproval>
                <AIEmpireGrowth>1.4</AIEmpireGrowth>
                <AIEmpireProduction>1.1</AIEmpireProduction>
                <AIEmpireResearch>1.8</AIEmpireResearch>
                <AIEmpireMoney>1.3</AIEmpireMoney>
                <AIEmpireEmpirePoint>1.1</AIEmpireEmpirePoint>
                <AIEmpirePillageDefense>1.1</AIEmpirePillageDefense>
                <AIEmpireAntiSpy>1.2</AIEmpireAntiSpy>
                <AIEmpireMilitaryPower>1.1</AIEmpireMilitaryPower>
                <AIEmpireCityDefense>1.5</AIEmpireCityDefense>
                <AIEmpireVision>1.5</AIEmpireVision>
                <AIEmpireBuyout>1.1</AIEmpireBuyout>
                <AIEmpireImprovedRuinSearch>1</AIEmpireImprovedRuinSearch>
                <AIEmpireStrategicResource>1.4</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.6</AIEmpireLuxuryResource>
                <AIEmpireUnlockShip>1</AIEmpireUnlockShip>
                <AIEmpireUnlockUnit>1.1</AIEmpireUnlockUnit>
                <AIEmpireUnlockAssimilation>1.1</AIEmpireUnlockAssimilation>
                <AIEmpireUnlockBribe>1.1</AIEmpireUnlockBribe>
                <AIEmpireUnlockPillage>0.8</AIEmpireUnlockPillage>
                <AIEmpireUnlockPrivateers>0.9</AIEmpireUnlockPrivateers>
                <AIEmpireUnlockMarketUnit>1.1</AIEmpireUnlockMarketUnit>
                <AIEmpireUnlockMarketResource>1.5</AIEmpireUnlockMarketResource>
                <AIEmpireUnlockBooster>1.5</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>0</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>1.2</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>              
              <ElementEvaluatorContextMultiplier>
                <AICityApproval>1.1</AICityApproval>
                <AICityGrowth>1.1</AICityGrowth>
                <AICityProduction>1.6</AICityProduction>
                <AICityResearch>1.7</AICityResearch>
                <AICityMoney>1.2</AICityMoney>
                <AICityEmpirePoint>1.1</AICityEmpirePoint>
                <AICityMoneyUpkeep>1.1</AICityMoneyUpkeep>
                <AICityDefense>1.6</AICityDefense>
                <AICityPillageDefense>1.6</AICityPillageDefense>
                <AICityLuxuryResource>1.4</AICityLuxuryResource>
                <AICityStrategicResource>1.4</AICityStrategicResource>
                <AICityAntiSpy>1.3</AICityAntiSpy>
                <AIEmpireMilitaryPower>1.9</AIEmpireMilitaryPower>
                <AIEmpireVision>1.4</AIEmpireVision>
                <AIEmpireWonderVictory>0.8</AIEmpireWonderVictory>
                <AIEmpireUnlockAltarOfAuriga>1.1</AIEmpireUnlockAltarOfAuriga>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Production>
          </AIEntity_City>
        </MajorEmpire>
      </AI>

      <!-- AIStrategicPlanDefinition Database -->
      <AIStrategicPlanDefinition>
      </AIStrategicPlanDefinition>

    </Multipliers>
  </AIPersonalityDefinition>

</Datatable>
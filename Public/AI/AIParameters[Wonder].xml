<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  
  <!-- Museum of Auriga -->
  <AIParameterDatatableElement Name="DistrictWonder1">
    <AIParameter Name="CitySciencePercentHappy" Value="0.20" />
    <AIParameter Name="CityDustPercentHappy" Value="0.20" />
    <AIParameter Name="CityApproval" Value="20" /> <!-- Approximation of the 20 per level -->
  </AIParameterDatatableElement>
  <AIParameterDatatableElement Name="FreeDistrictWonder1">
    <AIParameter Name="CitySciencePercentHappy" Value="0.20" />
    <AIParameter Name="CityDustPercentHappy" Value="0.20" />
    <AIParameter Name="CityApproval" Value="20" />
    <!-- Approximation of the 20 per level -->
  </AIParameterDatatableElement>

  <!-- Industrial megapole -->
  <AIParameterDatatableElement Name="DistrictWonder2">
    <AIParameter Name="DistrictIndustry"     Value="25"/> <!-- Approximation of the 25 per level -->
  </AIParameterDatatableElement>
  <AIParameterDatatableElement Name="FreeDistrictWonder2">
    <AIParameter Name="DistrictIndustry"     Value="25"/>
    <!-- Approximation of the 25 per level -->
  </AIParameterDatatableElement>

  <!-- Alchemic Institute -->
  <AIParameterDatatableElement Name="DistrictWonder3">
    <AIParameter Name="DistrictScience"     Value="25"/> <!-- Approximation of the 25 per level -->
  </AIParameterDatatableElement>
  <AIParameterDatatableElement Name="FreeDistrictWonder3">
    <AIParameter Name="DistrictScience"     Value="25"/>
    <!-- Approximation of the 25 per level -->
  </AIParameterDatatableElement>

  <!-- Throne of Emperors -->
  <AIParameterDatatableElement Name="DistrictWonder4">
    <AIParameter Name="DistrictCityPoint"     Value="50"/> <!-- Approximation of the 50 per level -->
  </AIParameterDatatableElement>
  <AIParameterDatatableElement Name="FreeDistrictWonder4">
    <AIParameter Name="DistrictCityPoint"     Value="50"/>
    <!-- Approximation of the 50 per level -->
  </AIParameterDatatableElement>

  <!-- Reliquary of Auriga -->
  <AIParameterDatatableElement Name="DistrictWonder5">
    <AIParameter Name="DistrictDust"                    Value="100"/> <!-- Approximation of the 100 per level -->
    <AIParameter Name="EmpireMilitaryUpkeepPercent"     Value="0.5"/>
  </AIParameterDatatableElement>
  <AIParameterDatatableElement Name="FreeDistrictWonder5">
    <AIParameter Name="DistrictDust"                    Value="100"/>
    <!-- Approximation of the 100 per level -->
    <AIParameter Name="EmpireMilitaryUpkeepPercent"     Value="0.5"/>
  </AIParameterDatatableElement>

</Datatable>
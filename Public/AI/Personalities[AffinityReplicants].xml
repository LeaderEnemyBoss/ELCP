<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <AIPersonalityDefinition Name="PersonalityAffinityReplicants">
    <Multipliers>
      <!-- AI Registry -->
      <AI>
		<AgentDefinition>
		  
			<MoneyPrintTermAgent>
                <Multiplier>0</Multiplier>
            </MoneyPrintTermAgent>
			
			 <!-- 
			Money-grubbing
			-->
			<MoneyReferenceRatioAgent>
			  <MoneyStockToMaintainUpkeepTurnCount>3</MoneyStockToMaintainUpkeepTurnCount>
			  <MoneyIncomeGrowthPercent>3</MoneyIncomeGrowthPercent>
			</MoneyReferenceRatioAgent>
			<ResearchAgreementTermAgent>
			  <Multiplier>0</Multiplier>
			</ResearchAgreementTermAgent>
        </AgentDefinition>
       

        <MajorEmpire>
			<!-- ELCP\LeaderEnemyBoss this was missingh and probably a bug-->
			<AIEntity_Empire>
			<!-- End ELCP-->
          <!-- The degree to which various attitude modifiers are taken into account -->
				<AILayer_Victory>
					<VictoryFocusMilitary>1.4</VictoryFocusMilitary>
					<VictoryFocusDiplomacy>1.5</VictoryFocusDiplomacy>
					<VictoryFocusEconomy>2.5</VictoryFocusEconomy>
					<VictoryFocusTechnology>0</VictoryFocusTechnology>
				</AILayer_Victory>
				
          <AILayer_Attitude>
            <!-- ##### -->
            <!-- TRUST -->
            <!-- ##### -->
            <!-- Positive -->
            <AttitudeScorePeaceful>0.7</AttitudeScorePeaceful>
            <AttitudeScoreCommonEnemy>1</AttitudeScoreCommonEnemy>
            <AttitudeScoreCommonFriend>1</AttitudeScoreCommonFriend>
            <AttitudeScoreCommonAlly>1</AttitudeScoreCommonAlly>
            <AttitudeScorePositiveContract>0.9</AttitudeScorePositiveContract>
            <AttitudeScoreGift>0.9</AttitudeScoreGift>
            <AttitudeScoreMantaAspiratingInMyTerritory>0.9</AttitudeScoreMantaAspiratingInMyTerritory>
            <AttitudeScoreLongTermAlliance>0.7</AttitudeScoreLongTermAlliance>
            <AttitudeScoreLongTermPeace>0.7</AttitudeScoreLongTermPeace>
            <AttitudeScoreTerraformedInMyTerritoryPositive>0</AttitudeScoreTerraformedInMyTerritoryPositive>
            <AttitudeScoreTerraformedNearMyArmiesPositive>0</AttitudeScoreTerraformedNearMyArmiesPositive>
            <!-- Negative -->
            <AttitudeScoreLastWar>1.3</AttitudeScoreLastWar>
            <AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>1.2</AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>
            <AttitudeScoreAggressiveColonization>0.9</AttitudeScoreAggressiveColonization>
            <AttitudeScoreNegativeContract>1.2</AttitudeScoreNegativeContract>
            <AttitudeScoreVillageConverted>1.3</AttitudeScoreVillageConverted>
            <AttitudeScoreRegionalBuildingDestroyed>1</AttitudeScoreRegionalBuildingDestroyed>
            <AttitudeScoreSpy>0.8</AttitudeScoreSpy>
            <AttitudeScorePillaging>0.6</AttitudeScorePillaging>
            <AttitudeScorePillageSucceed>0.6</AttitudeScorePillageSucceed>
            <AttitudeScoreDismantleDeviceSuffered>0.6</AttitudeScoreDismantleDeviceSuffered>
            <AttitudeScoreOrbsStolen>0.4</AttitudeScoreOrbsStolen>
            <AttitudeScoreEliminatedAcquaintance>0.3</AttitudeScoreEliminatedAcquaintance>
            <AttitudeScoreEliminatedAlly>0.7</AttitudeScoreEliminatedAlly>
            <AttitudeScoreEliminatedEnemy>0.3</AttitudeScoreEliminatedEnemy>
            <AttitudeScoreEliminatedFriend>0.7</AttitudeScoreEliminatedFriend>
            <AttitudeScoreCityTakenFromAlly>0.6</AttitudeScoreCityTakenFromAlly>
            <AttitudeScoreCityTakenFromEnemy>0.3</AttitudeScoreCityTakenFromEnemy>
            <AttitudeScoreCityTakenFromFriend>0.6</AttitudeScoreCityTakenFromFriend>
            <AttitudeScoreAtWarWithMyAlly>0.3</AttitudeScoreAtWarWithMyAlly>
            <AttitudeScoreAlliedWithMyEnemy>1.0</AttitudeScoreAlliedWithMyEnemy>
            <AttitudeScoreBetrayedYourAlly>0.1</AttitudeScoreBetrayedYourAlly>
            <AttitudeScoreBetrayedYourFriend>0.1</AttitudeScoreBetrayedYourFriend>
            <AttitudeScoreTerraformedInMyTerritoryNegative>0.5</AttitudeScoreTerraformedInMyTerritoryNegative>
            <AttitudeScoreTerraformedNearMyArmiesNegative>0.9</AttitudeScoreTerraformedNearMyArmiesNegative>
            <AttitudeScoreCreepingNodeUpgradeComplete>0.8</AttitudeScoreCreepingNodeUpgradeComplete>
            <AttitudeScoreKaijuAttacked>1.5</AttitudeScoreKaijuAttacked>
            <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>

            <!-- #### -->
            <!-- WAR -->
            <!-- #### -->
            <!-- Positive (towards asking for peace) -->
            <AttitudeScoreWarFatigue>1.5</AttitudeScoreWarFatigue>
            <AttitudeScoreMyEmpireUnitsKilledDuringWar>1.5</AttitudeScoreMyEmpireUnitsKilledDuringWar>
            <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>1.3</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
            <AttitudeScoreMyEmpireCitiesTakenDuringWar>1.5</AttitudeScoreMyEmpireCitiesTakenDuringWar>
            <AttitudeScoreMyEmpireFortressesTakenDuringWar>1.3</AttitudeScoreMyEmpireFortressesTakenDuringWar>
            <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>1.3</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
            <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>1.3</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
            <!-- Negative (towards remaining at war) -->
            <AttitudeScoreOtherEmpireUnitsKilledDuringWar>1.3</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
            <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>1.3</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
            <AttitudeScoreOtherEmpireCitiesTakenDuringWar>1.3</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
            <AttitudeScoreOtherEmpireFortressesTakenDuringWar>1.3</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
            <!-- #### -->
            <!-- FEAR -->
            <!-- #### -->
            <!-- Positive (towards avoiding war) -->
            <AttitudeScoreMyEmpireLeadMilitaryPower>1.8</AttitudeScoreMyEmpireLeadMilitaryPower>
            <AttitudeScoreMyEmpireLeadNavalMilitaryPower>1.2</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
            <!-- Negative (towards declaring war) -->
            <AttitudeScoreOtherEmpireLeadMilitaryPower>1.8</AttitudeScoreOtherEmpireLeadMilitaryPower>
            <AttitudeScoreUnitsInMyTerritory>1.8</AttitudeScoreUnitsInMyTerritory>
            <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>1.6</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
            <!-- #### -->
            <!-- ENVY -->
            <!-- #### -->
            <!-- Positive (towards avoiding war) -->
            <AttitudeScoreMyEmpireLeadScore>0.3</AttitudeScoreMyEmpireLeadScore>
            <AttitudeScoreMyEmpireLeadRegionCount>0.3</AttitudeScoreMyEmpireLeadRegionCount>
            <AttitudeScoreMyEmpireLeadNavalRegionCount>0.3</AttitudeScoreMyEmpireLeadNavalRegionCount>
            <!-- Negative (towards declaring war) -->
            <AttitudeScoreCommonFrontier>1.8</AttitudeScoreCommonFrontier>
            <AttitudeScoreOtherEmpireLeadScore>1.8</AttitudeScoreOtherEmpireLeadScore>
            <AttitudeScoreLivingSpaceTense>1.8</AttitudeScoreLivingSpaceTense>
            <AttitudeScoreOtherEmpireLeadRegionCount>1.8</AttitudeScoreOtherEmpireLeadRegionCount>
            <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
            <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
            <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
            <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
            <AttitudeScoreVictoryAlertDiplomacy>1</AttitudeScoreVictoryAlertDiplomacy>
            <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
            <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
            <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>
            <AttitudeScoreVictoryAlertQuest>1</AttitudeScoreVictoryAlertQuest>
            <AttitudeScoreFortressesOwnedInTheSameRegion>1.4</AttitudeScoreFortressesOwnedInTheSameRegion>
            <AttitudeScoreOtherEmpireHasUniqueFacility>1.2</AttitudeScoreOtherEmpireHasUniqueFacility>
            <AttitudeScoreOtherEmpireLeadNavalRegionCount>1.2</AttitudeScoreOtherEmpireLeadNavalRegionCount>
          </AILayer_Attitude>
			<AILayer_Diplomacy>
			  <MinimumNumberOfTurnsBetweenPropositions>1.2</MinimumNumberOfTurnsBetweenPropositions>
			  <MaximumNumberOfTurnsBetweenPropositions>1.2</MaximumNumberOfTurnsBetweenPropositions>
			  <SweetenDealThreshold>1.5</SweetenDealThreshold>
			  <EmergencyGangupLimit>1.25</EmergencyGangupLimit>
			</AILayer_Diplomacy>
          
            <!--
            Spy masters
            -->
            <AILayer_HeroAssignation>
              <MaximumHeroCount>1.5</MaximumHeroCount>
	      <!-- ELCP\LeaderEnemyBoss to account for changes in registry.xml (so they dont spam too much heroes)-->
              <MaximumTurnForHero>0.8</MaximumTurnForHero>
              <MaximumFractionOfHeroesSpy>3</MaximumFractionOfHeroesSpy>
              <MinimalTurnBeforeSpying>0.5</MinimalTurnBeforeSpying>
			  <!-- ELCP/LeaderEnemyBoss: needs vision to actually use spies -->
			  <Utility>
				<StealVision>
					<factorForInterestingArmies>2</factorForInterestingArmies>
				</StealVision>
			  </Utility>
			  <!-- End ELCP -->
            </AILayer_HeroAssignation>

            <AILayer_Strategy>

              <ResearchScoreFactor>0.0</ResearchScoreFactor>
              <EconomicScoreFactor>1.1</EconomicScoreFactor>
              <DiplomaticScoreFactor>1.2</DiplomaticScoreFactor>
              <PrepareWarScoreFactor>0.9</PrepareWarScoreFactor>

              <!--
              Raiders
              -->
              <Network>
                <MilitaryEmpireNeedAgent>
                  <GlobalUnitCountMultiplier>1.5</GlobalUnitCountMultiplier>

                  <!-- City unit need: NumberOfUnitPerCity = CLAMP((turn - CityUnitNeedStartTurn) / CityUnitNeedTurnRatio, MinimumNumberOfUnitPerCity, MaximumNumberOfUnitPerCity) -->
                  <CityUnitNeedStartTurn>1.2</CityUnitNeedStartTurn>
                  <CityUnitNeedTurnRatio>0.75</CityUnitNeedTurnRatio>
                  <MinimumNumberOfUnitPerCity>0</MinimumNumberOfUnitPerCity>
                  <MaximumNumberOfUnitPerCity>0.6</MaximumNumberOfUnitPerCity>

                  <!-- Roaming unit need: NumberOfRoamingUnit = CLAMP((turn - RoamingUnitNeedStartTurn) / RoamingUnitNeedTurnRatio, MinimumNumberOfRoamingUnit, MaximumNumberOfRoamingUnit) -->
                  <RoamingUnitNeedStartTurn>0</RoamingUnitNeedStartTurn>
                  <RoamingUnitNeedTurnRatio>0.7</RoamingUnitNeedTurnRatio>
                  <MinimumNumberOfRoamingUnit>1</MinimumNumberOfRoamingUnit>
                  <MaximumNumberOfRoamingUnit>1</MaximumNumberOfRoamingUnit>

                  <!-- War unit need: NumberOfWarUnit = CLAMP(WarUnitNeedOffset + (turn - WarUnitNeedStartTurn) / WarUnitNeedTurnRatio, MinimumNumberOfWarUnit, MaximumNumberOfWarUnit) -->
                  <WarUnitNeedOffset>1.3</WarUnitNeedOffset>
                  <WarUnitNeedStartTurn>1.1</WarUnitNeedStartTurn>
                  <WarUnitNeedTurnRatio>0.9</WarUnitNeedTurnRatio>
                  <MinimumNumberOfWarUnit>1</MinimumNumberOfWarUnit>
                  <MaximumNumberOfWarUnit>1</MaximumNumberOfWarUnit>

                  <!-- NumbeOfUnitWantedInTheEmpire = (NumberOfUnitPerCity + NumberOfRoamingUnit + NumberOfWarUnit) * GlobalUnitCountMultiplier -->
                </MilitaryEmpireNeedAgent>
              </Network>
            </AILayer_Strategy>
            
            <AILayer_Altar>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireGrowth>1.1</AIEmpireGrowth>
                <AIEmpireProduction>1.3</AIEmpireProduction>
                <AIEmpireMoney>2</AIEmpireMoney>
                <AIEmpireResearch>0</AIEmpireResearch>
                <AIEmpireEmpirePoint>1.5</AIEmpireEmpirePoint>
                <AIEmpireApproval>1</AIEmpireApproval>

                <AIEmpireMilitaryPower>1.3</AIEmpireMilitaryPower>
                <AIEmpireAntiSpy>1.3</AIEmpireAntiSpy>
                <AIEmpirePillageDefense>1.1</AIEmpirePillageDefense>
                <AIEmpireCityDefense>1.1</AIEmpireCityDefense>
                <AIEmpireStrategicResource>0.9</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>0.9</AIEmpireLuxuryResource>

                <AIWorldNavy>1.0</AIWorldNavy>
                <AIWorldSpy>2</AIWorldSpy>
                <AIWorldWar>1.5</AIWorldWar>
                <AIWorldPeace>0.4</AIWorldPeace>
                <AIWorldStrategicMarketPrice>-0.2</AIWorldStrategicMarketPrice>
                <AIWorldLuxuryMarketPrice>-0.2</AIWorldLuxuryMarketPrice>
                <AIWorldMercenaryMarketPrice>-0.9</AIWorldMercenaryMarketPrice>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Altar>
            
            <AILayer_Research>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireApproval>0.9</AIEmpireApproval>
                <AIEmpireGrowth>1.1</AIEmpireGrowth>
                <AIEmpireProduction>1.3</AIEmpireProduction>
                <AIEmpireResearch>0</AIEmpireResearch>
                <AIEmpireMoney>5</AIEmpireMoney>
                <AIEmpireEmpirePoint>1.5</AIEmpireEmpirePoint>
                <AIEmpirePillageDefense>1.1</AIEmpirePillageDefense>
                <AIEmpireAntiSpy>1</AIEmpireAntiSpy>
                <AIEmpireMilitaryPower>1.1</AIEmpireMilitaryPower>
                <AIEmpireCityDefense>1.1</AIEmpireCityDefense>
                <AIEmpireVision>1.3</AIEmpireVision>
                <AIEmpireBuyout>2</AIEmpireBuyout>
                <AIEmpireImprovedRuinSearch>1</AIEmpireImprovedRuinSearch>
                <AIEmpireStrategicResource>0.9</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>0.9</AIEmpireLuxuryResource>
                <AIEmpireUnlockShip>1.2</AIEmpireUnlockShip>
                <AIEmpireUnlockUnit>1.2</AIEmpireUnlockUnit>
                <AIEmpireUnlockAssimilation>0.8</AIEmpireUnlockAssimilation>
                <AIEmpireUnlockBribe>0.7</AIEmpireUnlockBribe>
                <AIEmpireUnlockPillage>5</AIEmpireUnlockPillage>
                <AIEmpireUnlockPrivateers>2</AIEmpireUnlockPrivateers>
                <AIEmpireUnlockMarketUnit>2.5</AIEmpireUnlockMarketUnit>
                <AIEmpireUnlockMarketResource>1.4</AIEmpireUnlockMarketResource>
                <AIEmpireUnlockBooster>1.2</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>1</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>2</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
			
			<AILayer_Assimilation>
				<Bos>1</Bos>
				<Ceratan>1</Ceratan>
				<Delvers>1.2</Delvers>
				<Erycis>1</Erycis>
				<Haunts>0.4</Haunts>
				<Kazanji>0.8</Kazanji>
				<Silics>1</Silics>
				<SistersOfMercy>1</SistersOfMercy>
				<Urces>1</Urces>
				<Jotus>1</Jotus>
				<Hurnas>1</Hurnas>
				<Gauran>1</Gauran>
				<Birdhive>0.7</Birdhive>
				<Geldirus>1</Geldirus>
				<EyelessOnes>1</EyelessOnes>
				<Dorgeshi>1</Dorgeshi>	
				<DawnShua>0.9</DawnShua>
			</AILayer_Assimilation>
			
			<AILayer_Colonization>
              <EmpireApprovalNeededToEngageColonization>1</EmpireApprovalNeededToEngageColonization>
              <AcceptedNumberOfUnPacifiedRegion>1.5</AcceptedNumberOfUnPacifiedRegion>
            </AILayer_Colonization>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>              
              <ElementEvaluatorContextMultiplier>
                <AICityApproval>0.8</AICityApproval>
                <AICityGrowth>0.8</AICityGrowth>
                <AICityProduction>1.1</AICityProduction>
                <AICityResearch>0.0</AICityResearch>
                <AICityMoney>2.0</AICityMoney>
                <AICityEmpirePoint>1.6</AICityEmpirePoint>
                <AICityMoneyUpkeep>2.0</AICityMoneyUpkeep>
                <AICityDefense>1.4</AICityDefense>
                <AICityPillageDefense>1.4</AICityPillageDefense>
                <AICityLuxuryResource>1.4</AICityLuxuryResource>
                <AICityStrategicResource>0.9</AICityStrategicResource>
                <AICityAntiSpy>1.6</AICityAntiSpy>
                <AIEmpireMilitaryPower>1.6</AIEmpireMilitaryPower>
                <AIEmpireVision>2.0</AIEmpireVision>
                <AIEmpireWonderVictory>0.6</AIEmpireWonderVictory>
                <AIEmpireUnlockAltarOfAuriga>1.4</AIEmpireUnlockAltarOfAuriga>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Production>
          </AIEntity_City>
        </MajorEmpire>
      </AI>

      <!-- AIStrategicPlanDefinition Database -->
      <AIStrategicPlanDefinition>
      </AIStrategicPlanDefinition>

    </Multipliers>
  </AIPersonalityDefinition>

</Datatable>
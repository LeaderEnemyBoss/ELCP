            <AttitudeScoreCityTakenFromFriend>0.1</AttitudeScoreCityTakenFromFriend>
            <AttitudeScoreAtWarWithMyAlly>0.1</AttitudeScoreAtWarWithMyAlly>
            <AttitudeScoreAlliedWithMyEnemy>0.7</AttitudeScoreAlliedWithMyEnemy>
            <AttitudeScoreBetrayedYourAlly>0.1</AttitudeScoreBetrayedYourAlly>
            <AttitudeScoreBetrayedYourFriend>0.1</AttitudeScoreBetrayedYourFriend>
            <AttitudeScoreTerraformedInMyTerritoryNegative>1.4</AttitudeScoreTerraformedInMyTerritoryNegative>
            <AttitudeScoreTerraformedNearMyArmiesNegative>1.4</AttitudeScoreTerraformedNearMyArmiesNegative>
            <AttitudeScoreCreepingNodeUpgradeComplete>0.8</AttitudeScoreCreepingNodeUpgradeComplete>
            <AttitudeScoreKaijuAttacked>1.5</AttitudeScoreKaijuAttacked>
            <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>

            <!-- #### -->
            <!-- WAR -->
            <!-- #### -->
            <!-- Positive (towards asking for peace) -->
            <AttitudeScoreWarFatigue>1.3</AttitudeScoreWarFatigue>
            <AttitudeScoreMyEmpireUnitsKilledDuringWar>1.3</AttitudeScoreMyEmpireUnitsKilledDuringWar>
            <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>1.3</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
            <AttitudeScoreMyEmpireCitiesTakenDuringWar>1.3</AttitudeScoreMyEmpireCitiesTakenDuringWar>
            <AttitudeScoreMyEmpireFortressesTakenDuringWar>1.6</AttitudeScoreMyEmpireFortressesTakenDuringWar>
            <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>1.6</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
            <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>1.6</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
            <!-- Negative (towards remaining at war) -->
            <AttitudeScoreOtherEmpireUnitsKilledDuringWar>1.3</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
            <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>1.3</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
            <AttitudeScoreOtherEmpireCitiesTakenDuringWar>1.3</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
            <AttitudeScoreOtherEmpireFortressesTakenDuringWar>1.6</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
            <!-- #### -->
            <!-- FEAR -->
            <!-- #### -->
            <!-- Positive (towards avoiding war) -->
            <AttitudeScoreMyEmpireLeadMilitaryPower>1.4</AttitudeScoreMyEmpireLeadMilitaryPower>
            <AttitudeScoreMyEmpireLeadNavalMilitaryPower>1.6</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
            <!-- Negative (towards declaring war) -->
            <AttitudeScoreOtherEmpireLeadMilitaryPower>1.4</AttitudeScoreOtherEmpireLeadMilitaryPower>
            <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>1.6</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
            <AttitudeScoreUnitsInMyTerritory>1.8</AttitudeScoreUnitsInMyTerritory>
            <!-- #### -->
            <!-- ENVY -->
            <!-- #### -->
            <!-- Positive (towards avoiding war) -->
            <AttitudeScoreMyEmpireLeadScore>0.9</AttitudeScoreMyEmpireLeadScore>
            <AttitudeScoreMyEmpireLeadRegionCount>0.9</AttitudeScoreMyEmpireLeadRegionCount>
            <AttitudeScoreMyEmpireLeadNavalRegionCount>1.3</AttitudeScoreMyEmpireLeadNavalRegionCount>
            <!-- Negative (towards declaring war) -->
            <AttitudeScoreCommonFrontier>0.4</AttitudeScoreCommonFrontier>
            <AttitudeScoreOtherEmpireLeadScore>1.5</AttitudeScoreOtherEmpireLeadScore>
            <AttitudeScoreLivingSpaceTense>0.5</AttitudeScoreLivingSpaceTense>
            <AttitudeScoreOtherEmpireLeadRegionCount>0.9</AttitudeScoreOtherEmpireLeadRegionCount>
            <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
            <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
            <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
            <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
            <AttitudeScoreVictoryAlertDiplomacy>1</AttitudeScoreVictoryAlertDiplomacy>
            <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
            <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
            <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>
            <AttitudeScoreVictoryAlertQuest>1</AttitudeScoreVictoryAlertQuest>
            <AttitudeScoreFortressesOwnedInTheSameRegion>1.8</AttitudeScoreFortressesOwnedInTheSameRegion>
            <AttitudeScoreOtherEmpireHasUniqueFacility>1.4</AttitudeScoreOtherEmpireHasUniqueFacility>
            <AttitudeScoreOtherEmpireLeadNavalRegionCount>1.5</AttitudeScoreOtherEmpireLeadNavalRegionCount>
          </AILayer_Attitude>

          

            <AILayer_Navy>
              <GameProgressMinTurn>0</GameProgressMinTurn>
              <GameProgressCurveName>NavalPriorityDefaultCurve</GameProgressCurveName>
            </AILayer_Navy>

            <AILayer_Strategy>

              <ResearchScoreFactor>1.0</ResearchScoreFactor>
              <EconomicScoreFactor>1.1</EconomicScoreFactor>
              <DiplomaticScoreFactor>1.2</DiplomaticScoreFactor>
              <PrepareWarScoreFactor>0.9</PrepareWarScoreFactor>

              <!--
              Raiders
              -->
              <Network>
                <MilitaryEmpireNeedAgent>
                  <GlobalUnitCountMultiplier>1.5</GlobalUnitCountMultiplier>

                  <!-- City unit need: NumberOfUnitPerCity = CLAMP((turn - CityUnitNeedStartTurn) / CityUnitNeedTurnRatio, MinimumNumberOfUnitPerCity, MaximumNumberOfUnitPerCity) -->
                  <CityUnitNeedStartTurn>1.2</CityUnitNeedStartTurn>
                  <CityUnitNeedTurnRatio>0.75</CityUnitNeedTurnRatio>
                  <MinimumNumberOfUnitPerCity>0</MinimumNumberOfUnitPerCity>
                  <MaximumNumberOfUnitPerCity>0.6</MaximumNumberOfUnitPerCity>

                  <!-- Roaming unit need: NumberOfRoamingUnit = CLAMP((turn - RoamingUnitNeedStartTurn) / RoamingUnitNeedTurnRatio, MinimumNumberOfRoamingUnit, MaximumNumberOfRoamingUnit) -->
                  <RoamingUnitNeedStartTurn>0</RoamingUnitNeedStartTurn>
                  <RoamingUnitNeedTurnRatio>0.7</RoamingUnitNeedTurnRatio>
                  <MinimumNumberOfRoamingUnit>1.5</MinimumNumberOfRoamingUnit>
                  <MaximumNumberOfRoamingUnit>1.5</MaximumNumberOfRoamingUnit>

                  <!-- War unit need: NumberOfWarUnit = CLAMP(WarUnitNeedOffset + (turn - WarUnitNeedStartTurn) / WarUnitNeedTurnRatio, MinimumNumberOfWarUnit, MaximumNumberOfWarUnit) -->
                  <WarUnitNeedOffset>1.3</WarUnitNeedOffset>
                  <WarUnitNeedStartTurn>1.1</WarUnitNeedStartTurn>
                  <WarUnitNeedTurnRatio>0.9</WarUnitNeedTurnRatio>
                  <MinimumNumberOfWarUnit>1.2</MinimumNumberOfWarUnit>
                  <MaximumNumberOfWarUnit>1.2</MaximumNumberOfWarUnit>

                  <!-- NumbeOfUnitWantedInTheEmpire = (NumberOfUnitPerCity + NumberOfRoamingUnit + NumberOfWarUnit) * GlobalUnitCountMultiplier -->
                </MilitaryEmpireNeedAgent>
              </Network>
            </AILayer_Strategy>
            
            <AILayer_Altar>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireGrowth>1</AIEmpireGrowth>
                <AIEmpireProduction>1</AIEmpireProduction>
                <AIEmpireMoney>1</AIEmpireMoney>
                <AIEmpireResearch>1</AIEmpireResearch>
                <AIEmpireEmpirePoint>1.5</AIEmpireEmpirePoint>
                <AIEmpireApproval>1</AIEmpireApproval>

                <AIEmpireMilitaryPower>1.3</AIEmpireMilitaryPower>
                <AIEmpireAntiSpy>1.3</AIEmpireAntiSpy>
                <AIEmpirePillageDefense>1.1</AIEmpirePillageDefense>
                <AIEmpireCityDefense>1.1</AIEmpireCityDefense>
                <AIEmpireStrategicResource>0.7</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>0.7</AIEmpireLuxuryResource>

                <AIWorldNavy>2.0</AIWorldNavy>
                <AIWorldSpy>1.2</AIWorldSpy>
                <AIWorldWar>1.5</AIWorldWar>
                <AIWorldPeace>0.4</AIWorldPeace>
                <AIWorldStrategicMarketPrice>-0.2</AIWorldStrategicMarketPrice>
                <AIWorldLuxuryMarketPrice>-0.2</AIWorldLuxuryMarketPrice>
                <AIWorldMercenaryMarketPrice>-0.9</AIWorldMercenaryMarketPrice>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Altar>
            
            <AILayer_Research>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireApproval>1</AIEmpireApproval>
                <AIEmpireGrowth>1</AIEmpireGrowth>
                <AIEmpireProduction>1</AIEmpireProduction>
                <AIEmpireResearch>1</AIEmpireResearch>
                <AIEmpireMoney>1</AIEmpireMoney>
                <AIEmpireEmpirePoint>2</AIEmpireEmpirePoint>
                <AIEmpirePillageDefense>1.5</AIEmpirePillageDefense>
                <AIEmpireAntiSpy>1.5</AIEmpireAntiSpy>
                <AIEmpireMilitaryPower>1.4</AIEmpireMilitaryPower>
                <AIEmpireCityDefense>1.5</AIEmpireCityDefense>
                <AIEmpireVision>1.3</AIEmpireVision>
                <AIEmpireBuyout>1</AIEmpireBuyout>
                <AIEmpireImprovedRuinSearch>1</AIEmpireImprovedRuinSearch>
                <AIEmpireStrategicResource>0.5</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>0.5</AIEmpireLuxuryResource>
                <AIEmpireUnlockShip>2.0</AIEmpireUnlockShip>
                <AIEmpireUnlockUnit>0.9</AIEmpireUnlockUnit>
                <AIEmpireUnlockSeafaringUnit>2.0</AIEmpireUnlockSeafaringUnit>
                <AIEmpireUnlockAssimilation>3</AIEmpireUnlockAssimilation>
                <AIEmpireUnlockBribe>0.8</AIEmpireUnlockBribe>
                <AIEmpireUnlockPillage>0.7</AIEmpireUnlockPillage>
                <AIEmpireUnlockPrivateers>0.25</AIEmpireUnlockPrivateers>
                <AIEmpireUnlockMarketUnit>1.0</AIEmpireUnlockMarketUnit>
                <AIEmpireUnlockMarketResource>1.6</AIEmpireUnlockMarketResource>
                <AIEmpireUnlockBooster>1</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>0</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>1.4</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
			
			<AILayer_Colonization>
			  <AcceptedNumberOfUnPacifiedRegion>2</AcceptedNumberOfUnPacifiedRegion>
			</AILayer_Colonization>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>              
              <ElementEvaluatorContextMultiplier>
                <AICityApproval>1.0</AICityApproval>
                <AICityGrowth>1.0</AICityGrowth>
                <AICityProduction>0.9</AICityProduction>
                <AICityResearch>1.0</AICityResearch>
                <AICityMoney>1.5</AICityMoney>
                <AICityEmpirePoint>1.5</AICityEmpirePoint>
                <AICityMoneyUpkeep>1.0</AICityMoneyUpkeep>
                <AICityDefense>1.4</AICityDefense>
                <AICityPillageDefense>1.3</AICityPillageDefense>
                <AICityLuxuryResource>0.7</AICityLuxuryResource>
                <AICityStrategicResource>0.7</AICityStrategicResource>
                <AICityAntiSpy>1.4</AICityAntiSpy>

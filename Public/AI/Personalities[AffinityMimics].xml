<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <AIPersonalityDefinition Name="PersonalityAffinityMimics">
    <Multipliers>
      <!-- AI Registry -->
      <AI>
      	<AgentDefinition>
			<IndustryReferenceTurnCountAgent>
				<MaximumTurnCount>2</MaximumTurnCount>
				<ProductionStressMinimumValue>0.5</ProductionStressMinimumValue>
			</IndustryReferenceTurnCountAgent>
		
			<MoneyPrintTermAgent>
				<Multiplier>0</Multiplier>
			</MoneyPrintTermAgent>

			<PopulationReferenceTurnCountAgent>
				<!-- Define the maximum criticy point. MaximumNumberOfTurnToReachNextPopulation = RangeRatio *  IdealNumberOfTurnToReachNextPopulation -->
				<RangeRatio>0.5</RangeRatio>
			</PopulationReferenceTurnCountAgent>
			
			<PeaceTermAgent>
                <Multiplier>1.3</Multiplier>
            </PeaceTermAgent>
			
			<WarTermAgent>
				<OpportunistMultiplier>1.1</OpportunistMultiplier>
			</WarTermAgent>
        </AgentDefinition>
      
        <MajorEmpire>
          <AIEntity_Empire>
		    <AILayer_Victory>
				<VictoryFocusMilitary>1.8</VictoryFocusMilitary>
				<VictoryFocusDiplomacy>1.5</VictoryFocusDiplomacy>
				<VictoryFocusEconomy>0.8</VictoryFocusEconomy>
				<VictoryFocusTechnology>1.1</VictoryFocusTechnology>
			</AILayer_Victory>

            <!-- The degree to which various attitude modifiers are taken into account -->
            <AILayer_Attitude>
              <!-- ##### -->
              <!-- TRUST -->
              <!-- ##### -->
              <!-- Positive -->
              <AttitudeScorePeaceful>1.8</AttitudeScorePeaceful>
              <AttitudeScoreCommonEnemy>1.7</AttitudeScoreCommonEnemy>
              <AttitudeScoreCommonFriend>1.2</AttitudeScoreCommonFriend>
              <AttitudeScoreCommonAlly>1.3</AttitudeScoreCommonAlly>
              <AttitudeScorePositiveContract>1.5</AttitudeScorePositiveContract>
              <AttitudeScoreGift>1.3</AttitudeScoreGift>
              <AttitudeScoreMantaAspiratingInMyTerritory>0.6</AttitudeScoreMantaAspiratingInMyTerritory>
              <AttitudeScoreLongTermAlliance>0.9</AttitudeScoreLongTermAlliance>
              <AttitudeScoreLongTermPeace>0.9</AttitudeScoreLongTermPeace>
              <AttitudeScoreTerraformedInMyTerritoryPositive>0</AttitudeScoreTerraformedInMyTerritoryPositive>
              <AttitudeScoreTerraformedNearMyArmiesPositive>0</AttitudeScoreTerraformedNearMyArmiesPositive>
              <AttitudeScoreKaijuPlacedNearMyCityPositive>1.6</AttitudeScoreKaijuPlacedNearMyCityPositive>
              <!-- Negative -->
              <AttitudeScoreLastWar>0.5</AttitudeScoreLastWar>
              <AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>1</AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>
              <AttitudeScoreAggressiveColonization>1.1</AttitudeScoreAggressiveColonization>
              <AttitudeScoreNegativeContract>1.3</AttitudeScoreNegativeContract>
              <AttitudeScoreVillageConverted>1.8</AttitudeScoreVillageConverted>
              <AttitudeScoreRegionalBuildingDestroyed>1.8</AttitudeScoreRegionalBuildingDestroyed>
              <AttitudeScoreSpy>2</AttitudeScoreSpy>
              <AttitudeScorePillaging>1.2</AttitudeScorePillaging>
              <AttitudeScorePillageSucceed>1.5</AttitudeScorePillageSucceed>
              <AttitudeScoreDismantleCreepingNodeSuffered>1</AttitudeScoreDismantleCreepingNodeSuffered>
              <AttitudeScoreDismantleDeviceSuffered>1.5</AttitudeScoreDismantleDeviceSuffered>
              <AttitudeScoreOrbsStolen>0.4</AttitudeScoreOrbsStolen>
              <AttitudeScoreEliminatedAcquaintance>0.7</AttitudeScoreEliminatedAcquaintance>
              <AttitudeScoreEliminatedAlly>0.8</AttitudeScoreEliminatedAlly>
              <AttitudeScoreEliminatedEnemy>0.8</AttitudeScoreEliminatedEnemy>
              <AttitudeScoreEliminatedFriend>0.8</AttitudeScoreEliminatedFriend>
              <AttitudeScoreCityTakenFromAlly>0.8</AttitudeScoreCityTakenFromAlly>
              <AttitudeScoreCityTakenFromEnemy>0.8</AttitudeScoreCityTakenFromEnemy>
              <AttitudeScoreCityTakenFromFriend>0.8</AttitudeScoreCityTakenFromFriend>
              <AttitudeScoreAtWarWithMyAlly>0.7</AttitudeScoreAtWarWithMyAlly>
              <AttitudeScoreAlliedWithMyEnemy>1.5</AttitudeScoreAlliedWithMyEnemy>
              <AttitudeScoreBetrayedYourAlly>0.5</AttitudeScoreBetrayedYourAlly>
              <AttitudeScoreBetrayedYourFriend>0.5</AttitudeScoreBetrayedYourFriend>
              <AttitudeScoreTerraformedInMyTerritoryNegative>0.5</AttitudeScoreTerraformedInMyTerritoryNegative>
              <AttitudeScoreTerraformedNearMyArmiesNegative>0.5</AttitudeScoreTerraformedNearMyArmiesNegative>
              <AttitudeScoreCreepingNodeUpgradeComplete>0.6</AttitudeScoreCreepingNodeUpgradeComplete>
              <AttitudeScoreKaijuAttacked>6</AttitudeScoreKaijuAttacked>
              <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>

              <!-- #### -->
              <!-- WAR -->
              <!-- #### -->
              <!-- Positive (towards asking for peace) -->
              <AttitudeScoreWarFatigue>0.4</AttitudeScoreWarFatigue>
              <AttitudeScoreMyEmpireUnitsKilledDuringWar>0.6</AttitudeScoreMyEmpireUnitsKilledDuringWar>
              <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>1.4</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreMyEmpireCitiesTakenDuringWar>1.2</AttitudeScoreMyEmpireCitiesTakenDuringWar>
              <AttitudeScoreMyEmpireFortressesTakenDuringWar>1.1</AttitudeScoreMyEmpireFortressesTakenDuringWar>
              <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>1.1</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
              <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>0.5</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
              <!-- Negative (towards remaining at war) -->
              <AttitudeScoreOtherEmpireUnitsKilledDuringWar>1.2</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
              <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>1.4</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreOtherEmpireCitiesTakenDuringWar>1.0</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
              <AttitudeScoreOtherEmpireFortressesTakenDuringWar>1.2</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
              <!-- #### -->
              <!-- FEAR -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadMilitaryPower>1.5</AttitudeScoreMyEmpireLeadMilitaryPower>
              <AttitudeScoreMyEmpireLeadNavalMilitaryPower>1.1</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreOtherEmpireLeadMilitaryPower>0.9</AttitudeScoreOtherEmpireLeadMilitaryPower>
              <AttitudeScoreUnitsInMyTerritory>1.8</AttitudeScoreUnitsInMyTerritory>
              <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>0.6</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
              <!-- #### -->
              <!-- ENVY -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadScore>0.7</AttitudeScoreMyEmpireLeadScore>
              <AttitudeScoreMyEmpireLeadRegionCount>0.5</AttitudeScoreMyEmpireLeadRegionCount>
              <AttitudeScoreMyEmpireLeadNavalRegionCount>0.5</AttitudeScoreMyEmpireLeadNavalRegionCount>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreCommonFrontier>1.2</AttitudeScoreCommonFrontier>
              <AttitudeScoreOtherEmpireLeadScore>1.3</AttitudeScoreOtherEmpireLeadScore>
              <AttitudeScoreLivingSpaceTense>1.6</AttitudeScoreLivingSpaceTense>
              <AttitudeScoreOtherEmpireLeadRegionCount>0.1</AttitudeScoreOtherEmpireLeadRegionCount>
              <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
              <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
              <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
              <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
              <AttitudeScoreVictoryAlertDiplomacy>1</AttitudeScoreVictoryAlertDiplomacy>
              <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
              <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
              <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>
              <AttitudeScoreVictoryAlertQuest>1</AttitudeScoreVictoryAlertQuest>
              <AttitudeScoreFortressesOwnedInTheSameRegion>1.5</AttitudeScoreFortressesOwnedInTheSameRegion>
              <AttitudeScoreOtherEmpireHasUniqueFacility>1.5</AttitudeScoreOtherEmpireHasUniqueFacility>
              <AttitudeScoreOtherEmpireLeadNavalRegionCount>1.1</AttitudeScoreOtherEmpireLeadNavalRegionCount>
            </AILayer_Attitude>

            <AILayer_Strategy>

              <ResearchScoreFactor>0.6</ResearchScoreFactor>
              <EconomicScoreFactor>1.2</EconomicScoreFactor>
              <DiplomaticScoreFactor>0.75</DiplomaticScoreFactor>
              <PrepareWarScoreFactor>1.0</PrepareWarScoreFactor>

              <!--
              Defensive
              -->

              <Network>
                <MilitaryEmpireNeedAgent>
                  <GlobalUnitCountMultiplier>1</GlobalUnitCountMultiplier>

                  <!-- City unit need: NumberOfUnitPerCity = CLAMP((turn - CityUnitNeedStartTurn) / CityUnitNeedTurnRatio, MinimumNumberOfUnitPerCity, MaximumNumberOfUnitPerCity) -->
                  <CityUnitNeedStartTurn>0.5</CityUnitNeedStartTurn>
                  <CityUnitNeedTurnRatio>0.75</CityUnitNeedTurnRatio>
                  <MinimumNumberOfUnitPerCity>0</MinimumNumberOfUnitPerCity>
                  <MaximumNumberOfUnitPerCity>1.6</MaximumNumberOfUnitPerCity>

                  <!-- Roaming unit need: NumberOfRoamingUnit = CLAMP((turn - RoamingUnitNeedStartTurn) / RoamingUnitNeedTurnRatio, MinimumNumberOfRoamingUnit, MaximumNumberOfRoamingUnit) -->
                  <RoamingUnitNeedStartTurn>0</RoamingUnitNeedStartTurn>
                  <RoamingUnitNeedTurnRatio>1.2</RoamingUnitNeedTurnRatio>
                  <MinimumNumberOfRoamingUnit>1</MinimumNumberOfRoamingUnit>
                  <MaximumNumberOfRoamingUnit>1</MaximumNumberOfRoamingUnit>

                  <!-- War unit need: NumberOfWarUnit = CLAMP(WarUnitNeedOffset + (turn - WarUnitNeedStartTurn) / WarUnitNeedTurnRatio, MinimumNumberOfWarUnit, MaximumNumberOfWarUnit) -->
                  <WarUnitNeedOffset>0.8</WarUnitNeedOffset>
                  <WarUnitNeedStartTurn>1.5</WarUnitNeedStartTurn>
                  <WarUnitNeedTurnRatio>1.5</WarUnitNeedTurnRatio>
                  <MinimumNumberOfWarUnit>0.9</MinimumNumberOfWarUnit>
                  <MaximumNumberOfWarUnit>0.9</MaximumNumberOfWarUnit>

                  <!-- NumbeOfUnitWantedInTheEmpire = (NumberOfUnitPerCity + NumberOfRoamingUnit + NumberOfWarUnit) * GlobalUnitCountMultiplier -->
                </MilitaryEmpireNeedAgent>
              </Network>
            </AILayer_Strategy>

            <AILayer_Altar>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireGrowth>1.2</AIEmpireGrowth>
                <AIEmpireProduction>0.8</AIEmpireProduction>
                <AIEmpireMoney>1.3</AIEmpireMoney>
                <AIEmpireResearch>1.5</AIEmpireResearch>
                <AIEmpireEmpirePoint>1.8</AIEmpireEmpirePoint>
                <AIEmpireApproval>1.5</AIEmpireApproval>

                <AIEmpireMilitaryPower>1</AIEmpireMilitaryPower>
                <AIEmpireAntiSpy>1</AIEmpireAntiSpy>
                <AIEmpirePillageDefense>0.6</AIEmpirePillageDefense>
                <AIEmpireCityDefense>2</AIEmpireCityDefense>
                <AIEmpireStrategicResource>1.5</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.75</AIEmpireLuxuryResource>

                <AIWorldNavy>0.4</AIWorldNavy>
                <AIWorldSpy>0.1</AIWorldSpy>
                <AIWorldWar>0.5</AIWorldWar>
                <AIWorldPeace>0.6</AIWorldPeace>
                <AIWorldStrategicMarketPrice>-0.3</AIWorldStrategicMarketPrice>
                <AIWorldLuxuryMarketPrice>-0.4</AIWorldLuxuryMarketPrice>
                <AIWorldMercenaryMarketPrice>0.4</AIWorldMercenaryMarketPrice>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Altar>

            <AILayer_Research>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireApproval>0.6</AIEmpireApproval>
                <AIEmpireGrowth>3</AIEmpireGrowth>
                <AIEmpireProduction>0.8</AIEmpireProduction>
                <AIEmpireResearch>3</AIEmpireResearch>
                <AIEmpireMoney>1.5</AIEmpireMoney>
                <AIEmpireEmpirePoint>1.2</AIEmpireEmpirePoint>
                <AIEmpirePillageDefense>0.6</AIEmpirePillageDefense>
                <AIEmpireAntiSpy>1.5</AIEmpireAntiSpy>
                <AIEmpireMilitaryPower>1.5</AIEmpireMilitaryPower>
                <AIEmpireCityDefense>1.5</AIEmpireCityDefense>
                <AIEmpireVision>0.5</AIEmpireVision>
                <AIEmpireBuyout>0.8</AIEmpireBuyout>
                <AIEmpireImprovedRuinSearch>1.5</AIEmpireImprovedRuinSearch>
                <AIEmpireStrategicResource>1.2</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.75</AIEmpireLuxuryResource>
                <AIEmpireUnlockShip>0.5</AIEmpireUnlockShip>
                <AIEmpireUnlockUnit>0.5</AIEmpireUnlockUnit>
                <AIEmpireUnlockAssimilation>2</AIEmpireUnlockAssimilation>
                <AIEmpireUnlockBribe>0.5</AIEmpireUnlockBribe>
                <AIEmpireUnlockPillage>1.4</AIEmpireUnlockPillage>
                <AIEmpireUnlockPrivateers>1.4</AIEmpireUnlockPrivateers>
                <AIEmpireUnlockMarketUnit>1.8</AIEmpireUnlockMarketUnit>
                <AIEmpireUnlockMarketResource>1.4</AIEmpireUnlockMarketResource>
                <AIEmpireUnlockBooster>2</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>1</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>2</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
			
			<AILayer_Assimilation>
				<Bos>1.3</Bos>
				<Ceratan>0.8</Ceratan>
				<Delvers>1</Delvers>
				<Erycis>1</Erycis>
				<Haunts>1</Haunts>
				<Kazanji>1</Kazanji>
				<Silics>1.2</Silics>
				<SistersOfMercy>0.9</SistersOfMercy>
				<Urces>1.2</Urces>
				<Jotus>0.8</Jotus>
				<Hurnas>0.8</Hurnas>
				<Gauran>1</Gauran>
				<Birdhive>1</Birdhive>
				<Geldirus>1</Geldirus>
				<EyelessOnes>0.8</EyelessOnes>
				<Dorgeshi>1.2</Dorgeshi>
				<DawnShua>1</DawnShua>
			</AILayer_Assimilation>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>
			  <ArmyThresholdTurns>10</ArmyThresholdTurns>
              <ElementEvaluatorContextMultiplier>
                <AICityApproval>1</AICityApproval>
                <AICityGrowth>3</AICityGrowth>
                <AICityProduction>1.2</AICityProduction>
                <AICityResearch>2</AICityResearch>
                <AICityMoney>1.5</AICityMoney>
                <AICityEmpirePoint>1.2</AICityEmpirePoint>
                <AICityMoneyUpkeep>0.9</AICityMoneyUpkeep>
                <AICityDefense>0.5</AICityDefense>
                <AICityPillageDefense>0.5</AICityPillageDefense>
                <AICityLuxuryResource>0.6</AICityLuxuryResource>
                <AICityStrategicResource>0.6</AICityStrategicResource>
                <AICityAntiSpy>0.5</AICityAntiSpy>
                <AIEmpireMilitaryPower>0.5</AIEmpireMilitaryPower>
                <AIEmpireVision>0.9</AIEmpireVision>
                <AIEmpireWonderVictory>1.2</AIEmpireWonderVictory>
                <AIEmpireUnlockAltarOfAuriga>2</AIEmpireUnlockAltarOfAuriga>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Production>
          </AIEntity_City>
        </MajorEmpire>
      </AI>

      <!-- AIStrategicPlanDefinition Database -->
      <AIStrategicPlanDefinition>
      </AIStrategicPlanDefinition>

    </Multipliers>
  </AIPersonalityDefinition>

</Datatable>
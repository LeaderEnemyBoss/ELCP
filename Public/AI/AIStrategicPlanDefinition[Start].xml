<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <AIStrategicPlanDefinition Name="Start" StrageticNetwork="Default">
    
    <Scorings Name="AccountManager">
      <!-- Money -->
      <!-- Not used anymore: There is only one account for money now.
      <AIScoring Name="MilitaryAccount"      Value="0.5"/>
      <AIScoring Name="EconomyAccount"       Value="0.5"/>
      <AIScoring Name="HeroAccount"          Value="0.01"/>
      -->

      <!-- Empire Point -->
      <AIScoring Name="DiplomacyAccount"  Value="0.01"/>
      <AIScoring Name="EmpirePlanAccount"  Value="1"/>
    </Scorings>

    <ParameterModifiers Name="Colonization">
      <AIModifier Name="AIRegionEsperanceDistrictFood"      Value="0.5"/>
      <AIModifier Name="AIRegionEsperanceDistrictIndustry"  Value="1"/>
      <AIModifier Name="AIRegionEsperanceDistrictDust"      Value="0.3"/>
      <AIModifier Name="AIRegionEsperanceDistrictScience"   Value="0.5"/>
      <AIModifier Name="AICityGrowth"                       Value="0.5"/>
      <AIModifier Name="AICityProduction"                   Value="1"/>
      <AIModifier Name="AICityMoney"                        Value="0.3"/>
      <AIModifier Name="AICityResearch"                     Value="0.5"/>
      <AIModifier Name="AICityEmpirePoint"                  Value="0.1"/>
      <AIModifier Name="AICityApproval"                     Value="0.1"/>
      <AIModifier Name="AIRegionDistance"                   Value="-0.5"/>
      <AIModifier Name="AIRegionConnectivity"               Value="1"/>
      <AIModifier Name="AIRegionStrategicResources"         Value="0.5"/>
      <AIModifier Name="AIRegionLuxuryResources"            Value="0.5"/>
      <AIModifier Name="AIRegionSize"                       Value="0.1"/>
      <AIModifier Name="AIRegionNewContinent"               Value="-0.5"/>
      <AIModifier Name="AIRegionNewContinentState"          Value="0.1"/>
      <AIModifier Name="AIRegionCurrentContinentState"      Value="-0.5"/>
      <AIModifier Name="AIRegionCurrentVillageState"        Value="0.2"/>
    </ParameterModifiers>

    <ParameterModifiers Name="EmpirePlan">
      <AIModifier Name="AIEmpirePlanEconomyAndPopulation1"  Value="1"/>
      <AIModifier Name="AIEmpirePlanEconomyAndPopulation2"  Value="1"/>
      <AIModifier Name="AIEmpirePlanEconomyAndPopulation3"  Value="1"/>
      <AIModifier Name="AIEmpirePlanEconomyAndPopulation4"  Value="1"/>

      <AIModifier Name="AIEmpirePlanEmpireAndExpansion1"  Value="0.1"/>
      <AIModifier Name="AIEmpirePlanEmpireAndExpansion2"  Value="1"/>
      <AIModifier Name="AIEmpirePlanEmpireAndExpansion3"  Value="1"/>
      <AIModifier Name="AIEmpirePlanEmpireAndExpansion4"  Value="1"/>

      <AIModifier Name="AIEmpirePlanMilitary1"  Value="0.1"/>
      <AIModifier Name="AIEmpirePlanMilitary2"  Value="0.1"/>
      <AIModifier Name="AIEmpirePlanMilitary3"  Value="0.1"/>
      <AIModifier Name="AIEmpirePlanMilitary4"  Value="0.1"/>

      <AIModifier Name="AIEmpirePlanScienceAndIndustry1"  Value="1"/>
      <AIModifier Name="AIEmpirePlanScienceAndIndustry2"  Value="1"/>
      <AIModifier Name="AIEmpirePlanScienceAndIndustry3"  Value="1"/>
      <AIModifier Name="AIEmpirePlanScienceAndIndustry4"  Value="1"/>
    </ParameterModifiers>

    <ParameterModifiers Name="ResourceBoosters">
      <AIModifier  Name="AIEmpireProduction"               Value="0.5"/>
      <AIModifier  Name="AIEmpireMoney"                    Value="0.5"/>
      <AIModifier  Name="AIEmpireResearch"                 Value="0.5"/>
      <AIModifier  Name="AIEmpireGrowth"                   Value="1.0"/>
      <AIModifier  Name="AIEmpireEmpirePoint"              Value="0.01"/>
      <AIModifier  Name="AICityDefense"                    Value="0.01"/>
      <AIModifier  Name="AIEmpireMilitaryPower"            Value="0.01"/>
    </ParameterModifiers>
    
    <ParameterModifiers Name="AffinityBoosters">
      <AIModifier  Name="AIEmpireProduction"               Value="0.7"/>
      <AIModifier  Name="AIEmpireMoney"                    Value="0.5"/>
      <AIModifier  Name="AIEmpireResearch"                 Value="0.5"/>
      <AIModifier  Name="AIEmpireGrowth"                   Value="1.0"/>
    </ParameterModifiers>

    <ParameterModifiers Name="Research">
      <AIModifier Name="AIEmpireApproval"           Value="0.4"/>
      <AIModifier Name="AIEmpireGrowth"             Value="1.0"/>
      <AIModifier Name="AIEmpireProduction"         Value="1.0"/>
      <AIModifier Name="AIEmpireResearch"           Value="0.6"/>
      <AIModifier Name="AIEmpireMoney"              Value="0.5"/>
      <AIModifier Name="AIEmpireEmpirePoint"        Value="0.2"/>
      <AIModifier Name="AIEmpirePillageDefense"     Value="0.01"/>
      <AIModifier Name="AIEmpireAntiSpy"            Value="0.01"/>
      <AIModifier Name="AIEmpireMilitaryPower"      Value="0.2"/>
      <AIModifier Name="AIEmpireCityDefense"        Value="0.01"/>
      <AIModifier Name="AIEmpireVision"             Value="0.2"/>
      <AIModifier Name="AIEmpireBuyout"             Value="0.4"/>
      <AIModifier Name="AIEmpireImprovedRuinSearch"             Value="0.01"/>
      <AIModifier Name="AIEmpireStrategicResource"          Value="0.6"/>
      <AIModifier Name="AIEmpireLuxuryResource"             Value="0.5"/>
      <AIModifier Name="AIEmpireUnlockShip"               Value="0.01"/>
      <AIModifier Name="AIEmpireUnlockUnit"               Value="0.03"/>
      <AIModifier Name="AIEmpireUnlockSeafaringUnit"      Value="0.1"/>
      <AIModifier Name="AIEmpireUnlockAssimilation"       Value="0.3"/>
      <AIModifier Name="AIEmpireUnlockBribe"              Value="0.5"/>
      <AIModifier Name="AIEmpireUnlockPillage"            Value="0.01"/>
      <AIModifier Name="AIEmpireUnlockPrivateers"          Value="0.01"/>
      <AIModifier Name="AIEmpireUnlockMarketUnit"         Value="0"/>
      <AIModifier Name="AIEmpireUnlockMarketResource"     Value="0.3"/>
      <AIModifier Name="AIEmpireUnlockBooster"            Value="0.1"/>
      <AIModifier Name="AIEmpireUnlockBoosterCultist"     Value="0.01"/>
      <AIModifier Name="AIEmpireUnlockDiplomacyPositive" Value="0.3"/>
    </ParameterModifiers>

    <ParameterModifiers Name="SeasonEffects">
      <!-- Register here the AI parameters you want to track for the season effects -->
      <AIModifier Name="AIEmpireGrowth"                     Value="1"/>
      <AIModifier Name="AIEmpireProduction"                 Value="0.9"/>
      <AIModifier Name="AIEmpireMoney"                      Value="0.3"/>
      <AIModifier Name="AIEmpireResearch"                   Value="0.4"/>
      <AIModifier Name="AIEmpireEmpirePoint"                Value="0.1"/>
      <AIModifier Name="AIEmpireApproval"                   Value="0.3"/>

      <AIModifier Name="AIEmpireMilitaryPower"              Value="0.1"/>
      <AIModifier Name="AIEmpireAntiSpy"                    Value="0.01"/>
      <AIModifier Name="AIEmpirePillageDefense"             Value="0.01"/>
      <AIModifier Name="AIEmpireCityDefense"                Value="0.01"/>
      <AIModifier Name="AIEmpireStrategicResource"          Value="0.2"/>
      <AIModifier Name="AIEmpireLuxuryResource"             Value="0.1"/>

      <AIModifier Name="AIWorldNavy"                        Value="0.2"/>
      <AIModifier Name="AIWorldSpy"                         Value="0.01"/>
      <AIModifier Name="AIWorldWar"                         Value="0.01"/>
      <AIModifier Name="AIWorldPeace"                       Value="0.01"/>
      <AIModifier Name="AIWorldStrategicMarketPrice"        Value="0.01"/>
      <AIModifier Name="AIWorldLuxuryMarketPrice"           Value="0.01"/>
      <AIModifier Name="AIWorldMercenaryMarketPrice"        Value="0.01"/>

    </ParameterModifiers>

    <AgentModifiers Name="Resource">
      <AgentModifier CurveReference="TechnologyReferenceTurnCount_Default">
        <AmasPath AgentReference="TechnologyReferenceTurnCount">ResourceEvaluationAmas</AmasPath>
      </AgentModifier>
      <AgentModifier CurveReference="MoneyReference_Default">
        <AmasPath AgentReference="MoneyReferenceRatio">ResourceEvaluationAmas</AmasPath>
      </AgentModifier>
      <AgentModifier CurveReference="EmpirePlanNeedPrestigeRatio_Default">
        <AmasPath AgentReference="EmpirePlanNeedPrestigeRatio">ResourceEvaluationAmas</AmasPath>
      </AgentModifier>
    </AgentModifiers>

    <CitySpecializations MaximalSpecializationPercentage="0.2">
      <CitySpecialization Name="Bank" MaximalSpecializationPercentage="1" SortingPropertyName="NetCityMoney"/>
    </CitySpecializations>

  </AIStrategicPlanDefinition>
  
</Datatable>

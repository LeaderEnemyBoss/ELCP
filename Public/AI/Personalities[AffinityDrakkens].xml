              <AttitudeScoreAtWarWithMyAlly>1.1</AttitudeScoreAtWarWithMyAlly>
              <AttitudeScoreAlliedWithMyEnemy>1.0</AttitudeScoreAlliedWithMyEnemy>
              <AttitudeScoreBetrayedYourAlly>1.4</AttitudeScoreBetrayedYourAlly>
              <AttitudeScoreBetrayedYourFriend>1.4</AttitudeScoreBetrayedYourFriend>
              <AttitudeScoreTerraformedInMyTerritoryNegative>1.0</AttitudeScoreTerraformedInMyTerritoryNegative>
              <AttitudeScoreTerraformedNearMyArmiesNegative>1.0</AttitudeScoreTerraformedNearMyArmiesNegative>
              <AttitudeScoreCreepingNodeUpgradeComplete>0.8</AttitudeScoreCreepingNodeUpgradeComplete>
              <AttitudeScoreKaijuAttacked>1.5</AttitudeScoreKaijuAttacked>
              <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>
              
              <!-- #### -->
              <!-- WAR -->
              <!-- #### -->
              <!-- Positive (towards asking for peace) -->
              <AttitudeScoreWarFatigue>0.9</AttitudeScoreWarFatigue>
              <AttitudeScoreMyEmpireUnitsKilledDuringWar>1.2</AttitudeScoreMyEmpireUnitsKilledDuringWar>
              <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>1.2</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreMyEmpireCitiesTakenDuringWar>1.2</AttitudeScoreMyEmpireCitiesTakenDuringWar>
              <AttitudeScoreMyEmpireFortressesTakenDuringWar>1.2</AttitudeScoreMyEmpireFortressesTakenDuringWar>
              <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>1.2</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
              <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>1.2</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
              <!-- Negative (towards remaining at war) -->
              <AttitudeScoreOtherEmpireUnitsKilledDuringWar>0.6</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
              <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>0.6</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreOtherEmpireCitiesTakenDuringWar>0.6</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
              <AttitudeScoreOtherEmpireFortressesTakenDuringWar>0.6</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
              <!-- #### -->
              <!-- FEAR -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadMilitaryPower>0.8</AttitudeScoreMyEmpireLeadMilitaryPower>
              <AttitudeScoreMyEmpireLeadNavalMilitaryPower>0.8</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreOtherEmpireLeadMilitaryPower>0.8</AttitudeScoreOtherEmpireLeadMilitaryPower>
              <AttitudeScoreUnitsInMyTerritory>0.7</AttitudeScoreUnitsInMyTerritory>
              <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>0.8</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
              <!-- #### -->
              <!-- ENVY -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadScore>1.3</AttitudeScoreMyEmpireLeadScore>
              <AttitudeScoreMyEmpireLeadRegionCount>1.1</AttitudeScoreMyEmpireLeadRegionCount>
              <AttitudeScoreMyEmpireLeadNavalRegionCount>1.1</AttitudeScoreMyEmpireLeadNavalRegionCount>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreCommonFrontier>0.7</AttitudeScoreCommonFrontier>
              <AttitudeScoreOtherEmpireLeadScore>1.6</AttitudeScoreOtherEmpireLeadScore>
              <AttitudeScoreLivingSpaceTense>0.9</AttitudeScoreLivingSpaceTense>
              <AttitudeScoreOtherEmpireLeadRegionCount>1.1</AttitudeScoreOtherEmpireLeadRegionCount>
              <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
              <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
              <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
              <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
              <AttitudeScoreVictoryAlertDiplomacy>1.5</AttitudeScoreVictoryAlertDiplomacy>
              <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
              <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
              <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>

                <AIEmpireUnlockBooster>1.2</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>0</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>1.4</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>
              <ElementEvaluatorContextMultiplier>
                <AICityApproval>1.6</AICityApproval>
                <AICityGrowth>1.2</AICityGrowth>
                <AICityProduction>1.1</AICityProduction>
                <AICityResearch>0.9</AICityResearch>
                <AICityMoney>0.9</AICityMoney>
                <AICityEmpirePoint>1.0</AICityEmpirePoint>
                <AICityMoneyUpkeep>1.0</AICityMoneyUpkeep>

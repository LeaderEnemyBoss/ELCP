<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <AIPersonalityDefinition Name="PersonalityAffinityVaulters">
    <Multipliers>
      <!-- AI Registry -->
      <AI>

        <AgentDefinition>

          <!-- 
          Architects
          -->
          <IndustryReferenceTurnCountAgent>
            <MaximumTurnCount>0.75</MaximumTurnCount>
            <ProductionStressMinimumValue>1</ProductionStressMinimumValue>
          </IndustryReferenceTurnCountAgent>

          <!--
          Isolationists
          -->
          <PopulationReferenceTurnCountAgent>
            <!-- MaximumNumberOfTurnToReachNextPopulation = RangeRatio *  IdealNumberOfTurnToReachNextPopulation -->
            <RangeRatio>1.25</RangeRatio>
          </PopulationReferenceTurnCountAgent>

          <!--
          Science-focused
          -->
          <TechnologyReferenceTurnCountAgent>
            <IdealTechnologyUnlockPeriod>0.75</IdealTechnologyUnlockPeriod>
            <MaximumPeriodGap>0.75</MaximumPeriodGap>
          </TechnologyReferenceTurnCountAgent>
		  
			<MoneyPrintTermAgent>
                <Multiplier>0</Multiplier>
            </MoneyPrintTermAgent>
			
		<ResearchAgreementTermAgent>
		    <Multiplier>1.6</Multiplier>
		</ResearchAgreementTermAgent>

        </AgentDefinition>

        <MajorEmpire>
          <AIEntity_Empire>

            <!-- The degree to which various attitude modifiers are taken into account -->
            <AILayer_Attitude>
              <!-- ##### -->
              <!-- TRUST -->
              <!-- ##### -->
              <!-- Positive -->
              <AttitudeScorePeaceful>1.2</AttitudeScorePeaceful>
              <AttitudeScoreCommonEnemy>1.4</AttitudeScoreCommonEnemy>
              <AttitudeScoreCommonFriend>1.2</AttitudeScoreCommonFriend>
              <AttitudeScoreCommonAlly>1.2</AttitudeScoreCommonAlly>
              <AttitudeScorePositiveContract>0.9</AttitudeScorePositiveContract>
              <AttitudeScoreGift>0.7</AttitudeScoreGift>
              <AttitudeScoreMantaAspiratingInMyTerritory>0.8</AttitudeScoreMantaAspiratingInMyTerritory>
              <AttitudeScoreLongTermAlliance>1.2</AttitudeScoreLongTermAlliance>
              <AttitudeScoreLongTermPeace>1.2</AttitudeScoreLongTermPeace>
              <AttitudeScoreTerraformedInMyTerritoryPositive>0.25</AttitudeScoreTerraformedInMyTerritoryPositive>
              <AttitudeScoreTerraformedNearMyArmiesPositive>0.25</AttitudeScoreTerraformedNearMyArmiesPositive>
              <AttitudeScoreKaijuPlacedNearMyCityPositive>0.6</AttitudeScoreKaijuPlacedNearMyCityPositive>
              <!-- Negative -->
              <AttitudeScoreLastWar>2</AttitudeScoreLastWar>
              <AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>2</AttitudeScoreArmyAggressionInNeutralRegionDuringColdWar>
              <AttitudeScoreAggressiveColonization>1.3</AttitudeScoreAggressiveColonization>
              <AttitudeScoreNegativeContract>1.5</AttitudeScoreNegativeContract>
              <AttitudeScoreVillageConverted>1.8</AttitudeScoreVillageConverted>
              <AttitudeScoreRegionalBuildingDestroyed>1.8</AttitudeScoreRegionalBuildingDestroyed>
              <AttitudeScoreSpy>1.8</AttitudeScoreSpy>
              <AttitudeScorePillaging>1.8</AttitudeScorePillaging>
              <AttitudeScorePillageSucceed>1.8</AttitudeScorePillageSucceed>
              <AttitudeScoreDismantleDeviceSuffered>1.8</AttitudeScoreDismantleDeviceSuffered>
              <AttitudeScoreOrbsStolen>1.3</AttitudeScoreOrbsStolen>
              <AttitudeScoreEliminatedAcquaintance>1.4</AttitudeScoreEliminatedAcquaintance>
              <AttitudeScoreEliminatedAlly>1.4</AttitudeScoreEliminatedAlly>
              <AttitudeScoreEliminatedEnemy>0.8</AttitudeScoreEliminatedEnemy>
              <AttitudeScoreEliminatedFriend>1.4</AttitudeScoreEliminatedFriend>
              <AttitudeScoreCityTakenFromAlly>1.4</AttitudeScoreCityTakenFromAlly>
              <AttitudeScoreCityTakenFromEnemy>0.8</AttitudeScoreCityTakenFromEnemy>
              <AttitudeScoreCityTakenFromFriend>1.4</AttitudeScoreCityTakenFromFriend>
              <AttitudeScoreAtWarWithMyAlly>0.8</AttitudeScoreAtWarWithMyAlly>
              <AttitudeScoreAlliedWithMyEnemy>1.8</AttitudeScoreAlliedWithMyEnemy>
              <AttitudeScoreBetrayedYourAlly>1.8</AttitudeScoreBetrayedYourAlly>
              <AttitudeScoreBetrayedYourFriend>1.8</AttitudeScoreBetrayedYourFriend>
              <AttitudeScoreTerraformedInMyTerritoryNegative>0</AttitudeScoreTerraformedInMyTerritoryNegative>
              <AttitudeScoreTerraformedNearMyArmiesNegative>0</AttitudeScoreTerraformedNearMyArmiesNegative>
              <AttitudeScoreCreepingNodeUpgradeComplete>0.8</AttitudeScoreCreepingNodeUpgradeComplete>
              <AttitudeScoreKaijuAttacked>1.5</AttitudeScoreKaijuAttacked>
              <AttitudeScoreKaijuPlacedNearMyCityNegative>0.4</AttitudeScoreKaijuPlacedNearMyCityNegative>
              
              <!-- #### -->
              <!-- WAR -->
              <!-- #### -->
              <!-- Positive (towards asking for peace) -->
              <AttitudeScoreWarFatigue>0.5</AttitudeScoreWarFatigue>
              <AttitudeScoreMyEmpireUnitsKilledDuringWar>0.7</AttitudeScoreMyEmpireUnitsKilledDuringWar>
              <AttitudeScoreMyEmpireCitiesBesiegedDuringWar>0.7</AttitudeScoreMyEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreMyEmpireCitiesTakenDuringWar>0.7</AttitudeScoreMyEmpireCitiesTakenDuringWar>
              <AttitudeScoreMyEmpireFortressesTakenDuringWar>0.7</AttitudeScoreMyEmpireFortressesTakenDuringWar>
              <AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>0.7</AttitudeScoreMyEmpireLostNavalRegionControlDuringWar>
              <AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>0.7</AttitudeScoreOtherEmpireLostNavalRegionControlDuringWar>
              <!-- Negative (towards remaining at war) -->
              <AttitudeScoreOtherEmpireUnitsKilledDuringWar>0.7</AttitudeScoreOtherEmpireUnitsKilledDuringWar>
              <AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>0.9</AttitudeScoreOtherEmpireCitiesBesiegedDuringWar>
              <AttitudeScoreOtherEmpireCitiesTakenDuringWar>0.9</AttitudeScoreOtherEmpireCitiesTakenDuringWar>
              <AttitudeScoreOtherEmpireFortressesTakenDuringWar>0.7</AttitudeScoreOtherEmpireFortressesTakenDuringWar>
              <!-- #### -->
              <!-- FEAR -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadMilitaryPower>0.8</AttitudeScoreMyEmpireLeadMilitaryPower>
              <AttitudeScoreMyEmpireLeadNavalMilitaryPower>0.8</AttitudeScoreMyEmpireLeadNavalMilitaryPower>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreOtherEmpireLeadMilitaryPower>0.8</AttitudeScoreOtherEmpireLeadMilitaryPower>
              <AttitudeScoreUnitsInMyTerritory>1</AttitudeScoreUnitsInMyTerritory>
              <AttitudeScoreOtherEmpireLeadNavalMilitaryPower>0.8</AttitudeScoreOtherEmpireLeadNavalMilitaryPower>
              <!-- #### -->
              <!-- ENVY -->
              <!-- #### -->
              <!-- Positive (towards avoiding war) -->
              <AttitudeScoreMyEmpireLeadScore>1.3</AttitudeScoreMyEmpireLeadScore>
              <AttitudeScoreMyEmpireLeadRegionCount>1.3</AttitudeScoreMyEmpireLeadRegionCount>
              <AttitudeScoreMyEmpireLeadNavalRegionCount>1.1</AttitudeScoreMyEmpireLeadNavalRegionCount>
              <!-- Negative (towards declaring war) -->
              <AttitudeScoreCommonFrontier>1.7</AttitudeScoreCommonFrontier>
              <AttitudeScoreOtherEmpireLeadScore>0.8</AttitudeScoreOtherEmpireLeadScore>
              <AttitudeScoreLivingSpaceTense>1.7</AttitudeScoreLivingSpaceTense>
              <AttitudeScoreOtherEmpireLeadRegionCount>0.8</AttitudeScoreOtherEmpireLeadRegionCount>
              <AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>1</AttitudeScoreVictoryAlertGlobalScoreWhenLastTurnReached>
              <AttitudeScoreVictoryAlertLastEmpireStanding>1</AttitudeScoreVictoryAlertLastEmpireStanding>
              <AttitudeScoreVictoryAlertExpansion>1</AttitudeScoreVictoryAlertExpansion>
              <AttitudeScoreVictoryAlertEconomy>1</AttitudeScoreVictoryAlertEconomy>
              <AttitudeScoreVictoryAlertDiplomacy>1</AttitudeScoreVictoryAlertDiplomacy>
              <AttitudeScoreVictoryAlertWonder>1</AttitudeScoreVictoryAlertWonder>
              <AttitudeScoreVictoryAlertMostTechnologiesDiscovered>1.5</AttitudeScoreVictoryAlertMostTechnologiesDiscovered>
              <AttitudeScoreVictoryAlertSupremacy>1</AttitudeScoreVictoryAlertSupremacy>
              <AttitudeScoreVictoryAlertQuest>1</AttitudeScoreVictoryAlertQuest>
              <AttitudeScoreFortressesOwnedInTheSameRegion>1.7</AttitudeScoreFortressesOwnedInTheSameRegion>
              <AttitudeScoreOtherEmpireHasUniqueFacility>1.2</AttitudeScoreOtherEmpireHasUniqueFacility>
              <AttitudeScoreOtherEmpireLeadNavalRegionCount>0.9</AttitudeScoreOtherEmpireLeadNavalRegionCount>
            </AILayer_Attitude>
            
            <AILayer_Strategy>

              <ResearchScoreFactor>1.6</ResearchScoreFactor>
              <EconomicScoreFactor>0.9</EconomicScoreFactor>
              <DiplomaticScoreFactor>0.5</DiplomaticScoreFactor>
              <PrepareWarScoreFactor>1.2</PrepareWarScoreFactor>

              <!--
              Defensive
              -->
              <Network>
                <MilitaryEmpireNeedAgent>
                  <GlobalUnitCountMultiplier>1.3</GlobalUnitCountMultiplier>

                  <!-- City unit need: NumberOfUnitPerCity = CLAMP((turn - CityUnitNeedStartTurn) / CityUnitNeedTurnRatio, MinimumNumberOfUnitPerCity, MaximumNumberOfUnitPerCity) -->
                  <CityUnitNeedStartTurn>0.5</CityUnitNeedStartTurn>
                  <CityUnitNeedTurnRatio>0.8</CityUnitNeedTurnRatio>
                  <MinimumNumberOfUnitPerCity>0</MinimumNumberOfUnitPerCity>
                  <MaximumNumberOfUnitPerCity>1.5</MaximumNumberOfUnitPerCity>

                  <!-- Roaming unit need: NumberOfRoamingUnit = CLAMP((turn - RoamingUnitNeedStartTurn) / RoamingUnitNeedTurnRatio, MinimumNumberOfRoamingUnit, MaximumNumberOfRoamingUnit) -->
                  <RoamingUnitNeedStartTurn>0</RoamingUnitNeedStartTurn>
                  <RoamingUnitNeedTurnRatio>0.9</RoamingUnitNeedTurnRatio>
                  <MinimumNumberOfRoamingUnit>1.1</MinimumNumberOfRoamingUnit>
                  <MaximumNumberOfRoamingUnit>1.1</MaximumNumberOfRoamingUnit>

                  <!-- War unit need: NumberOfWarUnit = CLAMP(WarUnitNeedOffset + (turn - WarUnitNeedStartTurn) / WarUnitNeedTurnRatio, MinimumNumberOfWarUnit, MaximumNumberOfWarUnit) -->
                  <WarUnitNeedOffset>1.3</WarUnitNeedOffset>
                  <WarUnitNeedStartTurn>1.1</WarUnitNeedStartTurn>
                  <WarUnitNeedTurnRatio>1.1</WarUnitNeedTurnRatio>
                  <MinimumNumberOfWarUnit>2</MinimumNumberOfWarUnit>
                  <MaximumNumberOfWarUnit>1.1</MaximumNumberOfWarUnit>

                  <!-- NumbeOfUnitWantedInTheEmpire = (NumberOfUnitPerCity + NumberOfRoamingUnit + NumberOfWarUnit) * GlobalUnitCountMultiplier -->
                </MilitaryEmpireNeedAgent>
              </Network>
            </AILayer_Strategy>

            <!--
            Mind of metal and wheels
            -->
            <AILayer_ResourceManager>
              <MoneyFactor>0.8</MoneyFactor>
              <ProductionFactor>1.2</ProductionFactor>
              <StrategicResourceMinimumStockPerCity>1.5</StrategicResourceMinimumStockPerCity>
              <MaxDeviationFromResourceMinimumStock>0.5</MaxDeviationFromResourceMinimumStock>
            </AILayer_ResourceManager>

            <AILayer_Altar>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireGrowth>0.9</AIEmpireGrowth>
                <AIEmpireProduction>1.3</AIEmpireProduction>
                <AIEmpireMoney>1</AIEmpireMoney>
                <AIEmpireResearch>1.8</AIEmpireResearch>
                <AIEmpireEmpirePoint>0.8</AIEmpireEmpirePoint>
                <AIEmpireApproval>0.9</AIEmpireApproval>

                <AIEmpireMilitaryPower>1.5</AIEmpireMilitaryPower>
                <AIEmpireAntiSpy>1.4</AIEmpireAntiSpy>
                <AIEmpirePillageDefense>1.2</AIEmpirePillageDefense>
                <AIEmpireCityDefense>2</AIEmpireCityDefense>
                <AIEmpireStrategicResource>1.9</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.4</AIEmpireLuxuryResource>

                <AIWorldSpy>0.3</AIWorldSpy>
                <AIWorldWar>1.2</AIWorldWar>
                <AIWorldPeace>0.6</AIWorldPeace>
                <AIWorldStrategicMarketPrice>-0.1</AIWorldStrategicMarketPrice>
                <AIWorldLuxuryMarketPrice>-0.1</AIWorldLuxuryMarketPrice>
                <AIWorldMercenaryMarketPrice>-0.2</AIWorldMercenaryMarketPrice>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Altar>
            
            <AILayer_Research>
              <ElementEvaluatorContextMultiplier>
                <AIEmpireApproval>0.8</AIEmpireApproval>
                <AIEmpireGrowth>0.9</AIEmpireGrowth>
                <AIEmpireProduction>1.3</AIEmpireProduction>
                <AIEmpireResearch>1.8</AIEmpireResearch>
                <AIEmpireMoney>1</AIEmpireMoney>
                <AIEmpireEmpirePoint>0.8</AIEmpireEmpirePoint>
                <AIEmpirePillageDefense>1.2</AIEmpirePillageDefense>
                <AIEmpireAntiSpy>1.4</AIEmpireAntiSpy>
                <AIEmpireMilitaryPower>1.5</AIEmpireMilitaryPower>
                <AIEmpireCityDefense>2</AIEmpireCityDefense>
                <AIEmpireVision>1.8</AIEmpireVision>
                <AIEmpireBuyout>0.9</AIEmpireBuyout>
                <AIEmpireImprovedRuinSearch>1</AIEmpireImprovedRuinSearch>
                <AIEmpireStrategicResource>1.9</AIEmpireStrategicResource>
                <AIEmpireLuxuryResource>1.4</AIEmpireLuxuryResource>
                <AIEmpireUnlockShip>1</AIEmpireUnlockShip>
                <AIEmpireUnlockUnit>1.1</AIEmpireUnlockUnit>
                <AIEmpireUnlockAssimilation>0.8</AIEmpireUnlockAssimilation>
                <AIEmpireUnlockBribe>0.8</AIEmpireUnlockBribe>
                <AIEmpireUnlockPillage>0.7</AIEmpireUnlockPillage>
                <AIEmpireUnlockPrivateers>0.7</AIEmpireUnlockPrivateers>
                <AIEmpireUnlockMarketUnit>0.9</AIEmpireUnlockMarketUnit>
                <AIEmpireUnlockMarketResource>1.3</AIEmpireUnlockMarketResource>
                <AIEmpireUnlockBooster>1.2</AIEmpireUnlockBooster>
                <AIEmpireUnlockBoosterCultist>1</AIEmpireUnlockBoosterCultist>
                <AIEmpireUnlockDiplomacyPositive>0.8</AIEmpireUnlockDiplomacyPositive>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Research>
			
			<AILayer_Victory>
				<VictoryFocusMilitary>1.4</VictoryFocusMilitary>
				<VictoryFocusDiplomacy>1.5</VictoryFocusDiplomacy>
				<VictoryFocusEconomy>0.9</VictoryFocusEconomy>
				<VictoryFocusTechnology>2.5</VictoryFocusTechnology>
			</AILayer_Victory>
			
			<AILayer_Assimilation>
				<Bos>1</Bos>
				<Ceratan>1</Ceratan>
				<Delvers>1</Delvers>
				<Erycis>1</Erycis>
				<Haunts>1.3</Haunts>
				<Kazanji>1</Kazanji>
				<Silics>1.2</Silics>
				<SistersOfMercy>1</SistersOfMercy>
				<Urces>1</Urces>
				<Jotus>0.8</Jotus>
				<Hurnas>0.8</Hurnas>
				<Gauran>1</Gauran>
				<Birdhive>1</Birdhive>
				<Geldirus>1</Geldirus>
				<EyelessOnes>1</EyelessOnes>
				<Dorgeshi>1</Dorgeshi>	
				<DawnShua>0.7</DawnShua>
			</AILayer_Assimilation>
          </AIEntity_Empire>

          <AIEntity_City>
            <AILayer_Production>              
              <ElementEvaluatorContextMultiplier>
                <AICityGrowth>0.8</AICityGrowth>
                <AICityProduction>1.4</AICityProduction>
                <AICityResearch>1.8</AICityResearch>
                <AICityMoney>0.8</AICityMoney>
                <AICityEmpirePoint>0.6</AICityEmpirePoint>
                <AICityMoneyUpkeep>1.0</AICityMoneyUpkeep>
                <AICityDefense>1.8</AICityDefense>
                <AICityPillageDefense>1.8</AICityPillageDefense>
                <AICityLuxuryResource>1.2</AICityLuxuryResource>
                <AICityStrategicResource>1.7</AICityStrategicResource>
                <AICityAntiSpy>1.5</AICityAntiSpy>
                <AIEmpireMilitaryPower>1.6</AIEmpireMilitaryPower>
                <AIEmpireVision>1.8</AIEmpireVision>
                <AIEmpireWonderVictory>1.6</AIEmpireWonderVictory>
                <AIEmpireUnlockAltarOfAuriga>0.9</AIEmpireUnlockAltarOfAuriga>
              </ElementEvaluatorContextMultiplier>
            </AILayer_Production>
          </AIEntity_City>
        </MajorEmpire>
      </AI>

      <!-- AIStrategicPlanDefinition Database -->
      <AIStrategicPlanDefinition>
      </AIStrategicPlanDefinition>

    </Multipliers>
  </AIPersonalityDefinition>

</Datatable>